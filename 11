===== ./docker/docker-compose.postgres.yml =====
services:
  postgres:
    image: postgres:16-alpine
    container_name: gh-postgres
    restart: unless-stopped
    environment:
      POSTGRES_DB: gracehub
      POSTGRES_USER: gh_user
      POSTGRES_PASSWORD: gh_password_change_me
    ports:
      # ĞŸÑƒĞ±Ğ»Ğ¸ĞºÑƒĞµĞ¼ Ğ¿Ğ¾Ñ€Ñ‚ Ñ‚Ğ¾Ğ»ÑŒĞºĞ¾ Ğ½Ğ° localhost Ñ…Ğ¾ÑÑ‚Ğ°
      - "127.0.0.1:5432:5432"
    volumes:
      # Ğ”Ğ°Ğ½Ğ½Ñ‹Ğµ Postgres
      - postgres_data:/var/lib/postgresql/data
      # Ğ”Ğ¾Ğ¿Ğ¾Ğ»Ğ½Ğ¸Ñ‚ĞµĞ»ÑŒĞ½Ñ‹Ğµ ĞºĞ¾Ğ½Ñ„Ğ¸Ğ³Ğ¸ (01-tuning.conf Ğ¸ Ğ¿Ñ€.)
      - ./postgres/conf.d:/etc/postgresql/conf.d

volumes:
  postgres_data:


===== ./docker/postgres/conf.d/01-tuning.conf =====
# Ğ¡ĞµÑ‚ĞµĞ²Ñ‹Ğµ Ğ½Ğ°ÑÑ‚Ñ€Ğ¾Ğ¹ĞºĞ¸
listen_addresses = '*'
include_dir = 'conf.d'

# ĞŸĞ°Ğ¼ÑÑ‚ÑŒ
shared_buffers = 256MB
work_mem = 8MB
maintenance_work_mem = 128MB

# Ğ¡Ğ¾ĞµĞ´Ğ¸Ğ½ĞµĞ½Ğ¸Ñ
max_connections = 40

# WAL / Ñ‡ĞµĞºĞ¿Ğ¾Ğ¸Ğ½Ñ‚Ñ‹
wal_buffers = 8MB
checkpoint_timeout = 10min
max_wal_size = 512MB
min_wal_size = 128MB

# ĞĞ²Ñ‚Ğ¾Ğ²Ğ°ĞºÑƒÑƒĞ¼ (Ğ¾ÑÑ‚Ğ°Ğ²Ğ»ÑĞµĞ¼, Ğ½Ğ¾ Ğ½Ğµ Ğ°Ğ³Ñ€ĞµÑÑĞ¸Ğ²Ğ½Ñ‹Ğ¹)
