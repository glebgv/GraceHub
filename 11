===== ./frontend/miniapp_frontend/src/i18n/index.ts =====
// src/i18n/index.ts
import i18n from 'i18next';
import { initReactI18next } from 'react-i18next';

import ru from './locales/ru.json';
import en from './locales/en.json';
import es from './locales/es.json';
import hi from './locales/hi.json';
import zh from './locales/zh.json';

export const DEFAULT_LANG = 'ru';

i18n
  .use(initReactI18next)
  .init({
    resources: {
      ru: { translation: ru },
      en: { translation: en },
      es: { translation: es },
      hi: { translation: hi },
      zh: { translation: zh }
    },
    lng: DEFAULT_LANG,
    fallbackLng: 'en',
    supportedLngs: ['ru', 'en', 'es', 'hi', 'zh'],
    interpolation: {
      escapeValue: false
    }
  });

export default i18n;

===== ./frontend/miniapp_frontend/src/index.css =====
:root {
  font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
  line-height: 1.4;
  font-weight: 400;
  font-synthesis: none;
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;

  /* Ğ¦Ğ²ĞµÑ‚Ğ° Telegram Mini App (Ğ¿Ñ€Ğ¸ Ğ½Ğ°Ğ»Ğ¸Ñ‡Ğ¸Ğ¸) */
  color: var(--tg-theme-text-color, #111827);
  background-color: var(--tg-theme-bg-color, #f3f4f6);
}

/* Ğ¡Ğ±Ñ€Ğ¾Ñ ĞºĞ¾Ñ€Ğ¾Ğ±Ğ¾Ñ‡Ğ½Ğ¾Ğ¹ Ğ¼Ğ¾Ğ´ĞµĞ»Ğ¸ */
*,
*::before,
*::after {
  box-sizing: border-box;
}

/* Ğ‘Ğ°Ğ·Ğ¾Ğ²Ñ‹Ğµ Ñ€Ğ°Ğ·Ğ¼ĞµÑ€Ñ‹ Ğ´Ğ¾ĞºÑƒĞ¼ĞµĞ½Ñ‚Ğ° */
html,
body {
  margin: 0;
  padding: 0;
  height: 100%;
}

/* Ğ¤Ğ¾Ğ½ Ğ¸ Ğ±Ğ°Ğ·Ğ¾Ğ²Ñ‹Ğ¹ Ñ†Ğ²ĞµÑ‚ Ñ‚ĞµĞºÑÑ‚Ğ° */
body {
  min-width: 320px;
  min-height: 100vh;
  background-color: var(--tg-theme-bg-color, #f3f4f6);
  color: var(--tg-theme-text-color, #111827);
}

/* Ğ¡ÑÑ‹Ğ»ĞºĞ¸ */
a {
  font-weight: 500;
  color: var(--tg-theme-link-color, #3b82f6);
  text-decoration: none;
}

a:hover {
  text-decoration: underline;
}

/* Ğ—Ğ°Ğ³Ğ¾Ğ»Ğ¾Ğ²ĞºĞ¸ Ğ¿Ğ¾ ÑƒĞ¼Ğ¾Ğ»Ñ‡Ğ°Ğ½Ğ¸Ñ */
h1 {
  font-size: 1.6rem;
  line-height: 1.2;
  margin: 0;
}

/* Ğ‘Ğ°Ğ·Ğ¾Ğ²Ñ‹Ğ¹ ÑÑ‚Ğ¸Ğ»ÑŒ Ğ´Ğ»Ñ button â€” Ğ½Ğ¾ Ğ±ĞµĞ· Ğ¶Ñ‘ÑÑ‚ĞºĞ¾Ğ³Ğ¾ Ñ†ĞµĞ½Ñ‚Ñ€Ğ¸Ñ€Ğ¾Ğ²Ğ°Ğ½Ğ¸Ñ Ğ²ÑĞµĞ³Ğ¾ body */
button {
  border-radius: 999px;
  border: none;
  padding: 0.6em 1.2em;
  font-size: 0.95rem;
  font-weight: 500;
  font-family: inherit;
  background-color: var(--tg-theme-button-color, #3b82f6);
  color: var(--tg-theme-button-text-color, #ffffff);
  cursor: pointer;
  transition:
    background-color 0.15s ease,
    transform 0.08s ease,
    box-shadow 0.12s ease;
}

button:hover {
  background-color: rgba(59, 130, 246, 0.92);
  transform: translateY(-1px);
  box-shadow: 0 3px 6px rgba(37, 99, 235, 0.45);
}

button:active {
  transform: translateY(0);
  box-shadow: 0 1px 3px rgba(37, 99, 235, 0.35);
}

button:focus,
button:focus-visible {
  outline: 2px solid rgba(59, 130, 246, 0.65);
  outline-offset: 2px;
}

/* Ğ¡Ğ²ĞµÑ‚Ğ»Ğ°Ñ Ñ‚ĞµĞ¼Ğ° Ğ¿Ğ¾ ÑĞ¸ÑÑ‚ĞµĞ¼Ğ½Ñ‹Ğ¼ Ğ½Ğ°ÑÑ‚Ñ€Ğ¾Ğ¹ĞºĞ°Ğ¼ ĞºĞ°Ğº Ğ¼ÑĞ³ĞºĞ¸Ğ¹ Ğ¾Ğ²ĞµÑ€Ñ€Ğ°Ğ¹Ğ´, Ğ½Ğµ Ğ»Ğ¾Ğ¼Ğ°ÑÑ‰Ğ¸Ğ¹ Telegram */
@media (prefers-color-scheme: light) {
  :root {
    background-color: var(--tg-theme-bg-color, #ffffff);
    color: var(--tg-theme-text-color, #111827);
  }

  a:hover {
    color: var(--tg-theme-link-color, #2563eb);
  }
}

