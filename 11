    async def get_admin_menu(self) -> InlineKeyboardMarkup:
        # ĞĞ²Ñ‚Ğ¾Ğ¾Ñ‚Ğ²ĞµÑ‚Ñ‹
        autoreply_enabled = await self.get_setting("autoreply_enabled")
        autoreply_on = autoreply_enabled == "True"
        autoreply_label = f"{self.texts.menu_autoreply}: {'ğŸŸ¢' if autoreply_on else 'ğŸ”´'}"

        # Privacy Mode
        privacy_on = await self.is_privacy_enabled()
        privacy_label = f"Privacy Mode: {'ğŸŸ¢' if privacy_on else 'ğŸ”´'}"

        # Ğ¯Ğ·Ñ‹Ğº
        lang_code = await self.get_setting("lang_code") or "ru"
        lang_label = f"{self.texts.menu_language}: {lang_code.upper()}"

        return InlineKeyboardMarkup(
            inline_keyboard=[
                [
                    InlineKeyboardButton(
                        text=self.texts.menu_greeting,
                        callback_data="edit_greeting",
                    )
                ],
                [
                    InlineKeyboardButton(
                        text=autoreply_label,
                        callback_data="setup_autoreply",
                    )
                ],
                [
                    InlineKeyboardButton(
                        text=self.texts.menu_export_users,
                        callback_data="export_users",
                    )
                ],
                [
                    InlineKeyboardButton(
                        text=self.texts.menu_blacklist,
                        callback_data="blacklist",
                    )
                ],
                [
                    InlineKeyboardButton(
                        text=privacy_label,
                        callback_data="setup_privacy",
                    )
                ],
                [
                    InlineKeyboardButton(
                        text=lang_label,
                        callback_data="setup_language",
                    )
                ],
            ]
        )

    def get_blacklist_menu(self) -> InlineKeyboardMarkup:
        return InlineKeyboardMarkup(
            inline_keyboard=[
                [
                    InlineKeyboardButton(
                        text=self.texts.blacklist_btn_add,
                        callback_data="blacklist_add",
                    )
                ],
                [
                    InlineKeyboardButton(
                        text=self.texts.blacklist_btn_remove,
                        callback_data="blacklist_remove",
                    )
                ],
                [
                    InlineKeyboardButton(
                        text=self.texts.blacklist_btn_show,
                        callback_data="blacklist_show",
                    )
                ],
                [
                    InlineKeyboardButton(
                        text=self.texts.blacklist_btn_back,
                        callback_data="main_menu",
                    )
                ],
            ]
        )

    # ====================== ĞœĞĞŸĞŸĞ˜ĞĞ“ Ğ Ğ•ĞŸĞ›ĞĞ•Ğ’ ======================

    async def save_reply_mapping_v2(
        self,
        chat_id: int,
        message_id: int,
        target_user_id: int,
    ) -> None:
        now = datetime.now(timezone.utc)
        await self.db.execute(
            """
            INSERT INTO admin_reply_map_v2 (instance_id, chat_id, admin_message_id, target_user_id, created_at)
            VALUES (%s, %s, %s, %s, %s)
            ON CONFLICT (instance_id, chat_id, admin_message_id)
            DO UPDATE SET target_user_id = EXCLUDED.target_user_id,
                          created_at     = EXCLUDED.created_at
            """,
            (self.instance_id, chat_id, message_id, target_user_id, now),
        )


    async def get_target_user_by_admin_message(
        self,
        chat_id: int,
        admin_message_id: int,
    ) -> Optional[int]:
        row = await self.db.fetchone(
            """
            SELECT target_user_id
            FROM admin_reply_map_v2
            WHERE instance_id = %s AND chat_id = %s AND admin_message_id = %s
            """,
            (self.instance_id, chat_id, admin_message_id),
        )
        return int(row["target_user_id"]) if row else None


    # ====================== Ğ¢Ğ˜ĞšĞ•Ğ¢Ğ« / OPENCHAT ======================

    def _format_ticket_title(self, ticket: Dict[str, Any]) -> str:
        """
        Ğ¡Ğ¾Ğ±Ğ¸Ñ€Ğ°ĞµÑ‚ Ğ½Ğ°Ğ·Ğ²Ğ°Ğ½Ğ¸Ğµ Ñ‚ĞµĞ¼Ñ‹ Ñ‚Ğ¸ĞºĞµÑ‚Ğ° Ñ ÑƒÑ‡ĞµÑ‚Ğ¾Ğ¼ ÑÑ‚Ğ°Ñ‚ÑƒÑĞ° Ğ¸ Ğ½Ğ°Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ½Ğ¾Ğ³Ğ¾ Ğ¸ÑĞ¿Ğ¾Ğ»Ğ½Ğ¸Ñ‚ĞµĞ»Ñ.
        """
        status = ticket.get("status") or "new"
        emoji = self.STATUS_EMOJI.get(status, "â¬œï¸")

        user_label = ticket.get("username") or f"user {ticket.get('user_id')}"
        assignee = ticket.get("assigned_username")

        if assignee:
            if not assignee.startswith("@"):
                assignee = f"@{assignee}"
            return f"{emoji} #{ticket.get('id')} {user_label} â€¢ {assignee}"
        return f"{emoji} #{ticket.get('id')} {user_label}"


    async def update_ticket_topic_title(self, ticket: Dict[str, Any]) -> None:
        """
        ĞĞ±Ğ½Ğ¾Ğ²Ğ»ÑĞµÑ‚ Ğ½Ğ°Ğ·Ğ²Ğ°Ğ½Ğ¸Ğµ Ñ„Ğ¾Ñ€ÑƒĞ¼Ğ½Ğ¾Ğ¹ Ñ‚ĞµĞ¼Ñ‹ Ğ¿Ğ¾ Ğ´Ğ°Ğ½Ğ½Ñ‹Ğ¼ Ñ‚Ğ¸ĞºĞµÑ‚Ğ°.
        """
        thread_id = ticket.get("thread_id")   # Ğ±Ñ‹Ğ»Ğ¾ "threadid"
        chat_id = ticket.get("chat_id")
        if not thread_id or not chat_id:
            return

        try:
            await self.bot.edit_forum_topic(
                chat_id=chat_id,
                message_thread_id=thread_id,
                name=self._format_ticket_title(ticket),
            )
        except Exception as e:
            logger.error(
                "Failed to update topic title for ticket %s: %s",
                ticket.get("id"),
                e,
            )



    async def fetch_ticket(self, ticket_id: int) -> Optional[Dict[str, Any]]:
        row = await self.db.fetchone(
            """
            SELECT *
            FROM tickets
            WHERE instance_id = %s AND id = %s
            """,
            (self.instance_id, ticket_id),
        )
        return dict(row) if row else None


    async def set_ticket_status(
        self,
        ticket_id: int,
        status: str,
        assigned_username: Optional[str] = None,
        assigned_user_id: Optional[int] = None,
    ) -> None:
        now = datetime.now(timezone.utc)
        set_parts = ["status = %s", "updated_at = %s"]
        params: List[Any] = [status, now]

        if assigned_username is not None:
            set_parts.append("assigned_username = %s")
            params.append(assigned_username)
        if assigned_user_id is not None:
            set_parts.append("assigned_user_id = %s")
            params.append(assigned_user_id)
        if status == "closed":
            set_parts.append("closed_at = %s")
            params.append(now)

        params.extend([self.instance_id, ticket_id])

        sql = f"""
            UPDATE tickets
            SET {", ".join(set_parts)}
            WHERE instance_id = %s AND id = %s
        """
        await self.db.execute(sql, tuple(params))

        ticket = await self.fetch_ticket(ticket_id)
        if ticket:
            # Ğ¾Ğ±Ğ½Ğ¾Ğ²Ğ»ÑĞµĞ¼ Ğ·Ğ°Ğ³Ğ¾Ğ»Ğ¾Ğ²Ğ¾Ğº Ñ‚ĞµĞ¼Ñ‹
            await self.update_ticket_topic_title(ticket)

            # ĞµÑĞ»Ğ¸ Ñ‚Ğ¾Ğ»ÑŒĞºĞ¾ Ñ‡Ñ‚Ğ¾ Ğ·Ğ°ĞºÑ€Ñ‹Ğ»Ğ¸ â€” Ğ¾Ñ‚Ğ¿Ñ€Ğ°Ğ²Ğ»ÑĞµĞ¼ Ğ·Ğ°Ğ¿Ñ€Ğ¾Ñ Ğ¾Ñ†ĞµĞ½ĞºĞ¸
            if status == "closed":
                try:
                    user_id = ticket.get("user_id")
                    if user_id:
                        await self._send_safe_message(
                            chat_id=user_id,
                            text=self.texts.ticket_closed_rating_request,
                            reply_markup=self.get_rating_keyboard(ticket_id),
                        )
                except Exception as e:
                    logger.error(
                        "Failed to send rating request for ticket %s: %s",
                        ticket_id,
                        e,
                    )



    async def handle_rating_callback(self, cb: CallbackQuery) -> None:
        data = cb.data or ""
        if not data.startswith("rating:"):
            return

        parts = data.split(":", 2)
        if len(parts) != 3:
            await cb.answer()
            return

        try:
            ticket_id = int(parts[1])
        except ValueError:
            await cb.answer()
            return

        rating_emoji = parts[2]

        ticket = await self.fetch_ticket(ticket_id)
        if not ticket:
            await cb.answer(self.texts.ticket_not_found, show_alert=True)
            return

        thread_id = ticket.get("thread_id")
        chat_id = ticket.get("chat_id")
        if not thread_id or not chat_id:
            await cb.answer()
            return

        # ĞŸĞ¸ÑˆĞµĞ¼ ÑĞ¸ÑÑ‚ĞµĞ¼Ğ½Ğ¾Ğµ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğµ Ğ² Ñ‚Ğ¾Ğ¿Ğ¸Ğº
        try:
            await self.bot.send_message(
                chat_id=chat_id,
                text=self.texts.rating_topic_message.format(emoji=rating_emoji),
                message_thread_id=thread_id,
            )
        except Exception as e:
            logger.error(
                "Failed to send rating message for ticket %s: %s",
                ticket_id,
                e,
            )

        # ĞĞ±Ğ½Ğ¾Ğ²Ğ»ÑĞµĞ¼ Ñ‚ĞµĞºÑÑ‚ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ñ Ñ Ğ¾Ñ†ĞµĞ½ĞºĞ¾Ğ¹ Ğ´Ğ»Ñ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ
        try:
            if cb.message:
                await cb.message.edit_text(
                    self.texts.rating_thanks_edit,
                    reply_markup=None,
                )
        except Exception as e:
            logger.error(
                "Failed to edit rating prompt message for ticket %s: %s",
                ticket_id,
                e,
            )

        await cb.answer(self.texts.rating_thanks_alert)

    async def put_ticket_keyboard(
        self,
        ticket_id: int,
        message_id: int,
        *,
        compact: bool = True,
    ) -> None:
        ticket = await self.fetch_ticket(ticket_id)
        if not ticket or not self.db:
            return

        chat_id = ticket["chat_id"]
        status = ticket.get("status") or "new"
        is_spam = status == "spam"
        is_closed = status == "closed"
        can_close = status not in ("closed", "spam")

        if compact:
            kb = InlineKeyboardMarkup(
                inline_keyboard=[
                    [
                        InlineKeyboardButton(
                            text="ğŸ–²",
                            callback_data=f"ticket:menu:{ticket_id}",
                        )
                    ]
                ]
            )
        else:
            kb = self._build_full_ticket_keyboard(
                ticket_id,
                can_close,
                is_spam=is_spam,
                is_closed=is_closed,
            )

        try:
            await self.bot.edit_message_reply_markup(
                chat_id=chat_id,
                message_id=message_id,
                reply_markup=kb,
            )
        except Exception as e:
            if "message is not modified" in str(e).lower():
                return
            logger.error(
                "Failed to attach ticket keyboard to message %s: %s",
                message_id,
                e,
            )

    def _build_full_ticket_keyboard(
        self,
        ticket_id: int,
        can_close: bool,
        *,
        is_spam: bool = False,
        is_closed: bool = False,
    ) -> InlineKeyboardMarkup:
        """
        Ğ¡Ñ‚Ñ€Ğ¾Ğ¸Ñ‚ Ğ¿Ğ¾Ğ»Ğ½Ğ¾Ğµ Ğ¼ĞµĞ½Ñ Ñ‚Ğ¸ĞºĞµÑ‚Ğ°.
        Ğ”Ğ»Ñ spam: 'ĞĞµ ÑĞ¿Ğ°Ğ¼' + 'Ğ¡Ğ²ĞµÑ€Ğ½ÑƒÑ‚ÑŒ'.
        Ğ”Ğ»Ñ closed: 'ĞŸĞµÑ€ĞµĞ¾Ñ‚ĞºÑ€Ñ‹Ñ‚ÑŒ' + 'Ğ¡Ğ²ĞµÑ€Ğ½ÑƒÑ‚ÑŒ'.
        Ğ”Ğ»Ñ Ğ¾Ğ±Ñ‹Ñ‡Ğ½Ğ¾Ğ³Ğ¾: Ğ¡ĞµĞ±Ğµ / ĞĞ°Ğ·Ğ½Ğ°Ñ‡Ğ¸Ñ‚ÑŒ / Ğ¡Ğ¿Ğ°Ğ¼ / Ğ—Ğ°ĞºÑ€Ñ‹Ñ‚ÑŒ + 'Ğ¡Ğ²ĞµÑ€Ğ½ÑƒÑ‚ÑŒ'.
        """
        buttons: List[List[InlineKeyboardButton]] = []

        if is_spam:
            buttons.append(
                [
                    InlineKeyboardButton(
                        text=self.texts.ticket_btn_not_spam,
                        callback_data=f"ticket:not_spam:{ticket_id}",
                    )
                ]
            )
        elif is_closed:
            buttons.append(
                [
                    InlineKeyboardButton(
                        text=self.texts.ticket_btn_reopen,
                        callback_data=f"ticket:reopen:{ticket_id}",
                    )
                ]
            )
        else:
            buttons.append(
                [
                    InlineKeyboardButton(
                        text=self.texts.ticket_btn_self,
                        callback_data=f"ticket:self:{ticket_id}",
                    ),
                    InlineKeyboardButton(
                        text=self.texts.ticket_btn_assign,
                        callback_data=f"ticket:assign:{ticket_id}",
                    ),
                ]
            )
            row_spam: List[InlineKeyboardButton] = [
                InlineKeyboardButton(
                    text=self.texts.ticket_btn_spam,
                    callback_data=f"ticket:spam:{ticket_id}",
                )
            ]
            if can_close:
                row_spam.append(
                    InlineKeyboardButton(
                        text=self.texts.ticket_btn_close,
                        callback_data=f"ticket:close:{ticket_id}",
                    )
                )
            buttons.append(row_spam)

        # ĞšĞ½Ğ¾Ğ¿ĞºĞ° ÑĞ²ĞµÑ€Ğ½ÑƒÑ‚ÑŒ Ğ½Ğ°Ğ·Ğ°Ğ´ Ğ² ğŸ–²
        buttons.append(
            [
                InlineKeyboardButton(
                    text=self.texts.ticket_btn_compact,
                    callback_data=f"ticket:compact:{ticket_id}",
                )
            ]
        )

        return InlineKeyboardMarkup(inline_keyboard=buttons)


    async def fetch_ticket_by_chat(
        self,
        chat_id: int,
        username: str,
        user_id: int,
    ) -> Optional[Dict[str, Any]]:
        """
        ĞĞ°Ğ¹Ñ‚Ğ¸ ÑÑƒÑ‰ĞµÑÑ‚Ğ²ÑƒÑÑ‰Ğ¸Ğ¹ Ñ‚Ğ¸ĞºĞµÑ‚ Ğ´Ğ»Ñ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ Ğ² Ğ¿Ñ€Ğ¸Ğ²ÑĞ·Ğ°Ğ½Ğ½Ğ¾Ğ¼ Ñ‡Ğ°Ñ‚Ğµ.
        """
        if not self.db:
            return None

        if username:
            row = await self.db.fetchone(
                """
                SELECT *
                FROM tickets
                WHERE instance_id = %s AND chat_id = %s AND username = %s
                """,
                (self.instance_id, chat_id, username),
            )
        else:
            row = await self.db.fetchone(
                """
                SELECT *
                FROM tickets
                WHERE instance_id = %s AND chat_id = %s AND user_id = %s
                """,
                (self.instance_id, chat_id, user_id),
            )

        if not row:
            return None

        # row ÑƒĞ¶Ğµ DictCursor, Ğ¼Ğ¾Ğ¶Ğ½Ğ¾ Ğ¿Ñ€Ğ¾ÑÑ‚Ğ¾ dict(row)
        return dict(row)


    async def ensure_ticket_for_user(
        self,
        chat_id: int,
        user_id: int,
        username: str,
    ) -> Dict[str, Any]:
        """
        Ğ“Ğ°Ñ€Ğ°Ğ½Ñ‚Ğ¸Ñ€ÑƒĞµÑ‚ Ğ½Ğ°Ğ»Ğ¸Ñ‡Ğ¸Ğµ Ñ‚Ğ¸ĞºĞµÑ‚Ğ° Ğ² OpenChat Ğ´Ğ»Ñ Ğ´Ğ°Ğ½Ğ½Ğ¾Ğ³Ğ¾ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ.
        Ğ•ÑĞ»Ğ¸ Ñ‚Ğ¸ĞºĞµÑ‚ ÑƒĞ¶Ğµ ĞµÑÑ‚ÑŒ â€” Ğ²Ğ¾Ğ·Ğ²Ñ€Ğ°Ñ‰Ğ°ĞµÑ‚ ĞµĞ³Ğ¾, Ğ¸Ğ½Ğ°Ñ‡Ğµ ÑĞ¾Ğ·Ğ´Ğ°Ñ‘Ñ‚ Ğ½Ğ¾Ğ²Ñ‹Ğ¹ Ğ¸ Ñ‚Ğ¾Ğ¿Ğ¸Ğº.
        """

        # ĞŸÑ‹Ñ‚Ğ°ĞµĞ¼ÑÑ Ğ½Ğ°Ğ¹Ñ‚Ğ¸ ÑÑƒÑ‰ĞµÑÑ‚Ğ²ÑƒÑÑ‰Ğ¸Ğ¹ Ñ‚Ğ¸ĞºĞµÑ‚ Ğ´Ğ»Ñ ÑÑ‚Ğ¾Ğ³Ğ¾ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ Ğ² Ğ´Ğ°Ğ½Ğ½Ğ¾Ğ¼ Ñ‡Ğ°Ñ‚Ğµ
        ticket = await self.fetch_ticket_by_chat(chat_id, username, user_id)
        if ticket:
            return ticket

        # === Ğ‘Ğ˜Ğ›Ğ›Ğ˜ĞĞ“: Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼ Ğ»Ğ¸Ğ¼Ğ¸Ñ‚ Ñ‚Ğ¸ĞºĞµÑ‚Ğ¾Ğ² ===
        ok, reason = await self.db.increment_tickets_used(self.instance_id)
        if not ok:
            # Ğ·Ğ´ĞµÑÑŒ Ğ¼Ğ¾Ğ¶Ğ½Ğ¾ Ğ´Ğ¸Ñ„Ñ„ĞµÑ€ĞµĞ½Ñ†Ğ¸Ñ€Ğ¾Ğ²Ğ°Ñ‚ÑŒ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ñ, Ğ¿Ğ¾ĞºĞ° ÑĞ´ĞµĞ»Ğ°ĞµĞ¼ Ğ¿Ñ€Ğ¾ÑÑ‚Ğ¾Ğ¹ Ğ»Ğ¾Ğ³
            logger.warning(
                "Ticket creation blocked by billing: instance=%s reason=%s user_id=%s",
                self.instance_id,
                reason,
                user_id,
            )
            # ĞĞ¸Ñ‡ĞµĞ³Ğ¾ Ğ½Ğµ ÑĞ¾Ğ·Ğ´Ğ°Ñ‘Ğ¼. Ğ’Ñ‹ÑˆĞµ Ğ¿Ğ¾ ÑÑ‚ĞµĞºÑƒ Ñ‚Ñ‹ Ğ¼Ğ¾Ğ¶ĞµÑˆÑŒ Ğ¾Ñ‚Ñ€ĞµĞ°Ğ³Ğ¸Ñ€Ğ¾Ğ²Ğ°Ñ‚ÑŒ:
            # Ğ»Ğ¸Ğ±Ğ¾ Ğ¾Ñ‚Ğ¿Ñ€Ğ°Ğ²Ğ¸Ñ‚ÑŒ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ Ğ¿Ğ¾ÑÑĞ½ÑÑÑ‰ĞµĞµ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğµ, Ğ»Ğ¸Ğ±Ğ¾ Ñ‚Ğ¸Ñ…Ğ¾ Ğ¸Ğ³Ğ½Ğ¾Ñ€Ğ¸Ñ€Ğ¾Ğ²Ğ°Ñ‚ÑŒ.
            # Ğ”Ğ»Ñ ÑĞ¾Ğ²Ğ¼ĞµÑÑ‚Ğ¸Ğ¼Ğ¾ÑÑ‚Ğ¸ Ğ²ĞµÑ€Ğ½Ñ‘Ğ¼ Ğ¿ÑƒÑÑ‚Ğ¾Ğ¹ ÑĞ»Ğ¾Ğ²Ğ°Ñ€ÑŒ.
            return {
                "id": None,
                "user_id": user_id,
                "username": username,
                "chat_id": chat_id,
                "thread_id": None,
                "status": "billing_blocked",
                "assigned_username": None,
                "assigned_user_id": None,
                "billing_reason": reason,
            }

        now = datetime.now(timezone.utc)

        # Ğ¡Ğ¾Ğ·Ğ´Ğ°Ñ‘Ğ¼ Ğ±Ğ°Ğ·Ğ¾Ğ²Ñ‹Ğ¹ Ñ‚Ğ¸ĞºĞµÑ‚ Ğ² Postgres
        row = await self.db.fetchone(
            """
            INSERT INTO tickets (
                instance_id,
                user_id,
                username,
                chat_id,
                status,
                created_at,
                updated_at
            )
            VALUES (%s, %s, %s, %s, 'new', %s, %s)
            RETURNING id
            """,
            (self.instance_id, user_id, username, chat_id, now, now),
        )
        ticket_id = row["id"]

        # ĞŸÑ‹Ñ‚Ğ°ĞµĞ¼ÑÑ ÑĞ¾Ğ·Ğ´Ğ°Ñ‚ÑŒ Ñ„Ğ¾Ñ€ÑƒĞ¼Ğ½Ñ‹Ğ¹ Ñ‚Ğ¾Ğ¿Ğ¸Ğº Ğ¿Ğ¾Ğ´ ÑÑ‚Ğ¾Ğ³Ğ¾ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ
        thread_id = None
        user_label = username or f"user {user_id}"
        title = f"{ticket_id} Â· {user_label}"

        try:
            ft = await self.bot.create_forum_topic(chat_id, name=title)
            thread_id = ft.message_thread_id
            await self.db.execute(
                """
                UPDATE tickets
                   SET thread_id = %s, updated_at = %s
                 WHERE instance_id = %s AND id = %s
                """,
                (thread_id, now, self.instance_id, ticket_id),
            )
        except Exception as e:
            logger.error(f"Failed to create forum topic for ticket {ticket_id}: {e}")
            thread_id = None

        ticket = {
            "id": ticket_id,
            "user_id": user_id,
            "username": username,
            "chat_id": chat_id,
            "thread_id": thread_id,
            "status": "new",
            "assigned_username": None,
            "assigned_user_id": None,
        }

        return ticket


    # ====================== ĞĞĞ’ĞĞ¯ Ğ›ĞĞ“Ğ˜ĞšĞ: ĞšĞ›ĞĞ’Ğ Ğ¢ĞĞ›Ğ¬ĞšĞ ĞĞ ĞŸĞĞ¡Ğ›Ğ•Ğ”ĞĞ•Ğœ ======================

    async def _clear_ticket_keyboards_for_user(
        self,
        chat_id: int,
        user_id: int,
        exclude_message_id: int,
    ) -> None:
        """
        Ğ£Ğ±Ğ¸Ñ€Ğ°ĞµÑ‚ reply_markup Ñƒ Ğ²ÑĞµÑ… ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğ¹ ÑÑ‚Ğ¾Ğ³Ğ¾ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ Ğ² Ğ´Ğ°Ğ½Ğ½Ğ¾Ğ¼ OpenChat,
        ĞºÑ€Ğ¾Ğ¼Ğµ exclude_message_id. Ğ˜ÑĞ¿Ğ¾Ğ»ÑŒĞ·ÑƒĞµÑ‚ Ñ‚Ğ°Ğ±Ğ»Ğ¸Ñ†Ñƒ messages, ĞµÑĞ»Ğ¸ Ğ¾Ğ½Ğ° ĞµÑÑ‚ÑŒ.
        """
        if not self.db:
            return

        try:
            rows = await self.db.fetchall(
                """
                SELECT message_id
                FROM messages
                WHERE instance_id = %s
                AND chat_id = %s
                AND user_id = %s
                AND direction = 'user_to_openchat'
                AND message_id <> %s
                """,
                (self.instance_id, chat_id, user_id, exclude_message_id),
            )

        except Exception as e:
            logger.error(f"Failed to fetch messages for clearing keyboards: {e}")
            return

        for (mid,) in rows:
            try:
                await self.bot.edit_message_reply_markup(
                    chat_id=chat_id,
                    message_id=mid,
                    reply_markup=None,
                )
            except Exception:
                # Ğ¡Ğ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ñ Ğ¼Ğ¾Ğ³Ğ»Ğ¸ Ğ±Ñ‹Ñ‚ÑŒ ÑƒĞ´Ğ°Ğ»ĞµĞ½Ñ‹/Ğ½ĞµĞ´Ğ¾ÑÑ‚ÑƒĞ¿Ğ½Ñ‹ â€” Ğ¸Ğ³Ğ½Ğ¾Ñ€Ğ¸Ñ€ÑƒĞµĞ¼
                continue

    async def store_forwarded_message(self, chat_id: int, message: Message, user_id: int) -> None:
        text_content = None
        if message.text:
            text_content = message.text
        elif message.caption:
            text_content = message.caption

        try:
            await self.db.execute(
                """
                INSERT INTO messages (
                    instance_id, chat_id, message_id, user_id, direction, content
                )
                VALUES (%s, %s, %s, %s, %s, %s)
                """,
                (self.instance_id, chat_id, message.message_id, user_id, "user_to_openchat", text_content),
            )
        except Exception as e:
            logger.error(f"Failed to insert message into messages table: {e}")


    async def forward_to_openchat(self, message: Message) -> None:
        """
        ĞÑ‚Ğ¿Ñ€Ğ°Ğ²ĞºĞ° Ğ²Ñ…Ğ¾Ğ´ÑÑ‰ĞµĞ³Ğ¾ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ñ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ Ğ² Ğ¿Ñ€Ğ¸Ğ²ÑĞ·Ğ°Ğ½Ğ½Ñ‹Ğ¹ OpenChat (Ğ² ĞµĞ³Ğ¾ Ñ‚Ğ¾Ğ¿Ğ¸Ğº)
        Ñ ÑĞ¾Ñ…Ñ€Ğ°Ğ½ĞµĞ½Ğ¸ĞµĞ¼ Ğ¼Ğ°Ğ¿Ğ¿Ğ¸Ğ½Ğ³Ğ° Ğ´Ğ»Ñ Ğ¿Ğ¾ÑĞ»ĞµĞ´ÑƒÑÑ‰Ğ¸Ñ… Ñ€ĞµĞ¿Ğ»ĞµĞµĞ² Ğ°Ğ´Ğ¼Ğ¸Ğ½Ğ¸ÑÑ‚Ñ€Ğ°Ñ‚Ğ¾Ñ€Ğ°.
        """
        # ĞĞ´Ğ¼Ğ¸Ğ½Ğ° Ğ² OpenChat Ğ½Ğµ Ñ„Ğ¾Ñ€Ğ²Ğ°Ñ€Ğ´Ğ¸Ğ¼
        if message.from_user and await self.is_admin(message.from_user.id):
            return

        # Ğ§Ñ‘Ñ€Ğ½Ñ‹Ğ¹ ÑĞ¿Ğ¸ÑĞ¾Ğº: ĞµÑĞ»Ğ¸ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»ÑŒ Ğ·Ğ°Ğ±Ğ»Ğ¾ĞºĞ¸Ñ€Ğ¾Ğ²Ğ°Ğ½ â€” Ğ¸Ğ³Ğ½Ğ¾Ñ€Ğ¸Ñ€ÑƒĞµĞ¼
        if message.from_user and await self.is_user_blacklisted(message.from_user.id):
            try:
                await self._send_safe_message(
                    chat_id=message.chat.id,
                    text="âŒ Ğ’Ñ‹ Ğ·Ğ°Ğ±Ğ»Ğ¾ĞºĞ¸Ñ€Ğ¾Ğ²Ğ°Ğ½Ñ‹ Ğ¸ Ğ½Ğµ Ğ¼Ğ¾Ğ¶ĞµÑ‚Ğµ Ğ¾Ñ‚Ğ¿Ñ€Ğ°Ğ²Ğ»ÑÑ‚ÑŒ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ñ Ğ² Ğ¿Ğ¾Ğ´Ğ´ĞµÑ€Ğ¶ĞºÑƒ.",
                )
            except Exception as e:
                logger.error(f"Failed to notify blacklisted user {message.from_user.id}: {e}")
            return

        oc = await self.get_openchat_settings()
        if not (oc["enabled"] and oc["chat_id"]):
            return

        if not self.db:
            return

        user_id = message.from_user.id
        username = message.from_user.username or ""
        chat_id = oc["chat_id"]

        # --- Ğ‘Ğ˜Ğ›Ğ›Ğ˜ĞĞ“ / Ğ›Ğ˜ĞœĞ˜Ğ¢Ğ« ---
        ticket = await self.ensure_ticket_for_user(chat_id, user_id, username)

        # Ğ•ÑĞ»Ğ¸ ensure_ticket_for_user Ğ²ĞµÑ€Ğ½ÑƒĞ» ÑĞ¿ĞµÑ†-ÑÑ‚Ğ°Ñ‚ÑƒÑ Ğ±Ğ»Ğ¾ĞºĞ¸Ñ€Ğ¾Ğ²ĞºĞ¸ Ğ±Ğ¸Ğ»Ğ»Ğ¸Ğ½Ğ³Ğ¾Ğ¼
        if ticket.get("status") == "billing_blocked":
            reason = ticket.get("billing_reason")

            # Ğ’Ñ‹Ğ±Ğ¸Ñ€Ğ°ĞµĞ¼ Ñ‚ĞµĞºÑÑ‚Ñ‹ Ğ´Ğ»Ñ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ Ğ¸ Ğ²Ğ»Ğ°Ğ´ĞµĞ»ÑŒÑ†ĞµĞ² Ğ¿Ğ¾ Ğ¿Ñ€Ğ¸Ñ‡Ğ¸Ğ½Ğµ
            if reason == "limit_reached":
                user_text = getattr(
                    self.texts,
                    "billing_user_limit_reached_message",
                    "âš ï¸ Ğ›Ğ¸Ğ¼Ğ¸Ñ‚ Ğ¾Ğ±Ñ€Ğ°Ñ‰ĞµĞ½Ğ¸Ğ¹ Ğ¿Ğ¾ Ñ‚ĞµĞºÑƒÑ‰ĞµĞ¼Ñƒ Ñ‚Ğ°Ñ€Ğ¸Ñ„Ñƒ Ğ¸ÑÑ‡ĞµÑ€Ğ¿Ğ°Ğ½. ĞŸĞ¾Ğ¿Ñ€Ğ¾Ğ±ÑƒĞ¹Ñ‚Ğµ ÑĞ²ÑĞ·Ğ°Ñ‚ÑŒÑÑ Ñ Ğ²Ğ»Ğ°Ğ´ĞµĞ»ÑŒÑ†Ğ°Ğ¼Ğ¸ Ğ±Ğ¾Ñ‚Ğ° Ğ´Ñ€ÑƒĞ³Ğ¸Ğ¼Ğ¸ ÑĞ¿Ğ¾ÑĞ¾Ğ±Ğ°Ğ¼Ğ¸.",
                )
                owner_text = getattr(
                    self.texts,
                    "billing_owner_limit_reached_message",
                    "âš ï¸ Ğ›Ğ¸Ğ¼Ğ¸Ñ‚ Ñ‚Ğ¸ĞºĞµÑ‚Ğ¾Ğ² Ğ¿Ğ¾ Ñ‚ĞµĞºÑƒÑ‰ĞµĞ¼Ñƒ Ñ‚Ğ°Ñ€Ğ¸Ñ„Ñƒ Ğ¸ÑÑ‡ĞµÑ€Ğ¿Ğ°Ğ½. ĞĞ¾Ğ²Ñ‹Ğµ Ğ¾Ğ±Ñ€Ğ°Ñ‰ĞµĞ½Ğ¸Ñ Ğ½Ğµ Ğ¿Ğ¾Ğ¿Ğ°Ğ´Ğ°ÑÑ‚ Ğ² ÑĞ¸ÑÑ‚ĞµĞ¼Ñƒ Ğ¿Ğ¾Ğ´Ğ´ĞµÑ€Ğ¶ĞºĞ¸.",
                )
            elif reason == "expired":
                user_text = getattr(
                    self.texts,
                    "billing_user_demo_expired_message",
                    "â³ Ğ¢ĞµÑÑ‚Ğ¾Ğ²Ñ‹Ğ¹ Ñ‚Ğ°Ñ€Ğ¸Ñ„ ÑÑ‚Ğ¾Ğ³Ğ¾ Ğ±Ğ¾Ñ‚Ğ° Ğ·Ğ°ĞºĞ¾Ğ½Ñ‡Ğ¸Ğ»ÑÑ, Ğ½Ğ¾Ğ²Ñ‹Ğµ Ğ¾Ğ±Ñ€Ğ°Ñ‰ĞµĞ½Ğ¸Ñ Ğ²Ñ€ĞµĞ¼ĞµĞ½Ğ½Ğ¾ Ğ½Ğµ Ğ¿Ñ€Ğ¸Ğ½Ğ¸Ğ¼Ğ°ÑÑ‚ÑÑ.",
                )
                owner_text = getattr(
                    self.texts,
                    "billing_owner_demo_expired_message",
                    "â³ Ğ”ĞµĞ¼Ğ¾â€‘Ğ¿ĞµÑ€Ğ¸Ğ¾Ğ´ Ğ±Ğ¾Ñ‚Ğ° Ğ·Ğ°ĞºĞ¾Ğ½Ñ‡Ğ¸Ğ»ÑÑ. ĞĞ¾Ğ²Ñ‹Ğµ Ñ‚Ğ¸ĞºĞµÑ‚Ñ‹ Ğ½Ğµ ÑĞ¾Ğ·Ğ´Ğ°ÑÑ‚ÑÑ.",
                )
            else:  # 'no_billing' Ğ¸Ğ»Ğ¸ Ğ¸Ğ½Ğ¾Ğµ
                user_text = getattr(
                    self.texts,
                    "billing_user_no_plan_message",
                    "âš ï¸ Ğ”Ğ»Ñ ÑÑ‚Ğ¾Ğ³Ğ¾ Ğ±Ğ¾Ñ‚Ğ° ĞµÑ‰Ñ‘ Ğ½Ğµ Ğ½Ğ°ÑÑ‚Ñ€Ğ¾ĞµĞ½ Ñ‚Ğ°Ñ€Ğ¸Ñ„ Ğ¿Ğ¾Ğ´Ğ´ĞµÑ€Ğ¶ĞºĞ¸, Ğ½Ğ¾Ğ²Ñ‹Ğµ Ğ¾Ğ±Ñ€Ğ°Ñ‰ĞµĞ½Ğ¸Ñ Ğ²Ñ€ĞµĞ¼ĞµĞ½Ğ½Ğ¾ Ğ½Ğµ Ğ¿Ñ€Ğ¸Ğ½Ğ¸Ğ¼Ğ°ÑÑ‚ÑÑ.",
                )
                owner_text = getattr(
                    self.texts,
                    "billing_owner_no_plan_message",
                    "âš ï¸ Ğ”Ğ»Ñ ÑÑ‚Ğ¾Ğ³Ğ¾ Ğ±Ğ¾Ñ‚Ğ° Ğ½Ğµ Ğ½Ğ°ÑÑ‚Ñ€Ğ¾ĞµĞ½ Ğ°ĞºÑ‚Ğ¸Ğ²Ğ½Ñ‹Ğ¹ Ñ‚Ğ°Ñ€Ğ¸Ñ„, Ğ¾Ğ±Ñ€Ğ°Ñ‰ĞµĞ½Ğ¸Ñ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»ĞµĞ¹ Ğ½Ğµ Ğ´Ğ¾Ñ…Ğ¾Ğ´ÑÑ‚ Ğ´Ğ¾ ÑĞ¸ÑÑ‚ĞµĞ¼Ñ‹ Ğ¿Ğ¾Ğ´Ğ´ĞµÑ€Ğ¶ĞºĞ¸.",
                )

            # Ğ¡Ğ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğµ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ (Ğ¾Ğ±ĞµĞ·Ğ»Ğ¸Ñ‡ĞµĞ½Ğ½Ğ¾Ğµ Ğ¿Ñ€Ğ¾ Ğ²Ğ»Ğ°Ğ´ĞµĞ»ÑŒÑ†ĞµĞ²)
            try:
                await self._send_safe_message(
                    chat_id=message.chat.id,
                    text=user_text,
                )
            except Exception as e:
                logger.error(
                    "Failed to notify user %s about billing limit (%s): %s",
                    user_id,
                    reason,
                    e,
                )

            # Ğ¡Ğ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğµ Ğ²Ğ»Ğ°Ğ´ĞµĞ»ÑŒÑ†Ğ°Ğ¼/Ğ¾Ğ¿ĞµÑ€Ğ°Ñ‚Ğ¾Ñ€Ğ°Ğ¼ Ğ² Generalâ€‘Ñ‚Ğ¾Ğ¿Ğ¸Ğº
            try:
                if oc["enabled"] and oc["chat_id"]:
                    await self.bot.send_message(
                        oc["chat_id"],
                        owner_text,
                    )
            except Exception as e:
                logger.error(
                    "Failed to notify owners in General about billing limit for instance %s: %s",
                    self.instance_id,
                    e,
                )

            return

        thread_id = ticket.get("thread_id")
        header = username or f"user {user_id}"

        now = datetime.now(timezone.utc)

        sent: Optional[Message] = None

        async def _send_into_thread(thread: int) -> Message:
            if message.text:
                body = f"{header}:\n{message.text}"
                return await self.bot.send_message(
                    chat_id,
                    body,
                    message_thread_id=thread,
                )
            elif message.photo:
                caption = message.caption or ""
                cap = f"{header}:\n{caption}" if caption else header
                return await self.bot.send_photo(
                    chat_id,
                    message.photo[-1].file_id,
                    caption=cap,
                    message_thread_id=thread,
                )
            elif message.video:
                caption = message.caption or ""
                cap = f"{header}:\n{caption}" if caption else header
                return await self.bot.send_video(
                    chat_id,
                    message.video.file_id,
                    caption=cap,
                    message_thread_id=thread,
                )
            elif message.document:
                caption = message.caption or ""
                cap = f"{header}:\n{caption}" if caption else header
                return await self.bot.send_document(
                    chat_id,
                    message.document.file_id,
                    caption=cap,
                    message_thread_id=thread,
                )
            elif message.audio:
                caption = message.caption or ""
                cap = f"{header}:\n{caption}" if caption else header
                return await self.bot.send_audio(
                    chat_id,
                    message.audio.file_id,
                    caption=cap,
                    message_thread_id=thread,
                )
            elif message.voice:
                return await self.bot.send_voice(
                    chat_id,
                    message.voice.file_id,
                    caption=header,
                    message_thread_id=thread,
                )
            elif message.sticker:
                return await self.bot.send_sticker(
                    chat_id,
                    message.sticker.file_id,
                    message_thread_id=thread,
                )
            else:
                body = f"{header}: [{message.content_type}]"
                return await self.bot.send_message(
                    chat_id,
                    body,
                    message_thread_id=thread,
                )

        # ĞŸÑ‹Ñ‚Ğ°ĞµĞ¼ÑÑ Ğ¾Ñ‚Ğ¿Ñ€Ğ°Ğ²Ğ¸Ñ‚ÑŒ Ğ² Ñ‚ĞµĞºÑƒÑ‰Ğ¸Ğ¹ thread_id
        try:
            sent = await _send_into_thread(thread_id)
        except Exception as e:
            err_text = str(e).lower()
            if "message thread not found" in err_text or "message thread not found" in getattr(
                getattr(e, "message", ""), "lower", lambda: ""
            )():
                # Ğ¢Ğ¾Ğ¿Ğ¸Ğº ÑƒĞ´Ğ°Ğ»Ñ‘Ğ½ â€” ÑĞ¾Ğ·Ğ´Ğ°Ñ‘Ğ¼ Ğ½Ğ¾Ğ²Ñ‹Ğ¹ Ğ¸ Ğ¾Ğ±Ğ½Ğ¾Ğ²Ğ»ÑĞµĞ¼ Ñ‚Ğ¸ĞºĞµÑ‚
                try:
                    ft = await self.bot.create_forum_topic(
                        chat_id,
                        name=self._format_ticket_title(ticket),
                    )
                    new_thread_id = ft.message_thread_id
                    await self.db.execute(
                        """
                        UPDATE tickets
                           SET thread_id = %s, updated_at = %s
                         WHERE instance_id = %s AND id = %s
                        """,
                        (new_thread_id, now, self.instance_id, ticket["id"]),
                    )
                    ticket["thread_id"] = new_thread_id
                    thread_id = new_thread_id

                    sent = await _send_into_thread(new_thread_id)
                except Exception as e2:
                    logger.error(f"Failed to recreate forum topic for ticket {ticket['id']}: {e2}")
                    return
            else:
                logger.error(f"Failed to forward to OpenChat: Telegram server says - {e}")
                return

        # Ğ¡Ğ¾Ñ…Ñ€Ğ°Ğ½ÑĞµĞ¼ ÑĞ²ÑĞ·ÑŒ Ğ´Ğ»Ñ ĞºĞ¾Ñ€Ñ€ĞµĞºÑ‚Ğ½Ğ¾Ğ³Ğ¾ Ñ€ĞµĞ¿Ğ»ĞµÑ Ğ°Ğ´Ğ¼Ğ¸Ğ½Ğ° ĞºĞ»Ğ¸ĞµĞ½Ñ‚Ñƒ
        if sent:
            # Ğ¼Ğ°Ğ¿Ğ¿Ğ¸Ğ½Ğ³ admin_message -> user
            await self.save_reply_mapping_v2(chat_id, sent.message_id, user_id)

            # ÑĞ¾Ñ…Ñ€Ğ°Ğ½ÑĞµĞ¼ Ğ·Ğ°Ğ¿Ğ¸ÑÑŒ Ğ² messages Ğ¾ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğ¸ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ Ğ² OpenChat
            await self.store_forwarded_message(
                chat_id=chat_id,
                message=sent,
                user_id=user_id,
            )

            # ÑĞ½Ğ°Ñ‡Ğ°Ğ»Ğ° ÑƒĞ±Ğ¸Ñ€Ğ°ĞµĞ¼ ĞºĞ»Ğ°Ğ²Ğ¸Ğ°Ñ‚ÑƒÑ€Ñƒ ÑĞ¾ Ğ²ÑĞµÑ… Ğ¿Ñ€ĞµĞ´Ñ‹Ğ´ÑƒÑ‰Ğ¸Ñ… ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğ¹ ÑÑ‚Ğ¾Ğ³Ğ¾ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ
            await self._clear_ticket_keyboards_for_user(
                chat_id=chat_id,
                user_id=user_id,
                exclude_message_id=sent.message_id,
            )

            # Ğ·Ğ°Ñ‚ĞµĞ¼ Ğ²ĞµÑˆĞ°ĞµĞ¼ ĞºĞ¾Ğ¼Ğ¿Ğ°ĞºÑ‚Ğ½ÑƒÑ ĞºĞ½Ğ¾Ğ¿ĞºÑƒ-Ğ¼ĞµĞ½Ñ Ñ‚Ğ¾Ğ»ÑŒĞºĞ¾ Ğ½Ğ° Ğ¿Ğ¾ÑĞ»ĞµĞ´Ğ½ĞµĞµ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğµ ĞºĞ»Ğ¸ĞµĞ½Ñ‚Ğ°
            await self.put_ticket_keyboard(ticket["id"], sent.message_id, compact=True)

        # ĞĞ±Ğ½Ğ¾Ğ²Ğ»ÑĞµĞ¼ Ñ‚Ğ°Ğ¹Ğ¼Ğ¸Ğ½Ğ³Ğ¸ Ñ‚Ğ¸ĞºĞµÑ‚Ğ°
        try:
            await self.db.execute(
                """
                UPDATE tickets
                   SET last_user_msg_at = %s,
                       updated_at       = %s
                 WHERE instance_id = %s
                   AND id          = %s
                """,
                (now, now, self.instance_id, ticket["id"]),
            )

            current_status = ticket.get("status") or "new"
            if current_status in ("answered", "closed"):
                await self.set_ticket_status(ticket["id"], "inprogress")
        except Exception as e:
            logger.error(f"Failed to update ticket timestamps: {e}")



    # ====================== ĞšĞĞœĞĞĞ”Ğ« ======================

    async def cmd_start(self, message: Message, state: FSMContext) -> None:
        user_id = message.from_user.id

        admin_id = await self.get_setting("admin_user_id")
        # ĞĞ²Ñ‚Ğ¾Ğ¼Ğ°Ñ‚Ğ¸Ñ‡ĞµÑĞºĞ¸ Ğ½Ğ°Ğ·Ğ½Ğ°Ñ‡Ğ°ĞµĞ¼ Ğ¿ĞµÑ€Ğ²Ğ¾Ğ³Ğ¾ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ Ğ°Ğ´Ğ¼Ğ¸Ğ½Ğ¾Ğ¼ (ĞµÑĞ»Ğ¸ ĞµÑ‰Ñ‘ Ğ½Ğµ Ğ·Ğ°Ğ´Ğ°Ğ½)
        if not admin_id or admin_id in ("0", ""):
            await self.set_setting("admin_user_id", str(user_id))
            await self._send_safe_message(
                chat_id=message.chat.id,
                text=self.texts.you_are_admin_now,
            )

        # ĞĞ±Ñ‰Ğ¸Ğµ Ğ½Ğ°ÑÑ‚Ñ€Ğ¾Ğ¹ĞºĞ¸ OpenChat Ğ´Ğ»Ñ ÑÑ‚Ğ°Ñ‚ÑƒÑĞ°
        oc = await self.get_openchat_settings()
        if oc["enabled"] and oc["chat_id"]:
            status_line_admin = self.texts.openchat_status_line_on
            status_line_user = self.texts.openchat_status_line_on
        else:
            status_line_admin = self.texts.openchat_status_line_off
            status_line_user = self.texts.openchat_status_line_off

        # Ğ’ĞµÑ‚ĞºĞ° Ğ´Ğ»Ñ Ğ°Ğ´Ğ¼Ğ¸Ğ½Ğ°
        if await self.is_admin(user_id):
            me = await self.bot.get_me()
            bot_username = me.username or "bot"

            if not oc["enabled"]:
                # Ğ”Ğ»Ñ Ğ½ĞµĞ·Ğ°Ğ½Ğ°ÑÑ‚Ñ€Ğ¾ĞµĞ½Ğ½Ğ¾Ğ³Ğ¾ OpenChat Ğ¿Ğ¾ĞºĞ°Ğ·Ñ‹Ğ²Ğ°ĞµĞ¼ ÑÑ‚Ğ°Ñ‚ÑƒÑ + Ğ¿Ğ¾Ğ´ÑĞºĞ°Ğ·ĞºÑƒ Ğ¿Ğ¾ Ğ¿Ñ€Ğ¸Ğ²ÑĞ·ĞºĞµ
                await self._send_safe_message(
                    chat_id=message.chat.id,
                    text=(
                        f"{status_line_admin}\n"
                        f"{self.texts.menu_you_are_admin}\n\n"
                        + self.texts.openchat_setup_hint.format(
                            bot_username=bot_username
                        )
                    ),
                )
            else:
                # ĞÑĞ½Ğ¾Ğ²Ğ½Ğ¾Ğµ Ğ°Ğ´Ğ¼Ğ¸Ğ½ÑĞºĞ¾Ğµ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğµ Ñ ĞºĞ»Ğ°Ğ²Ğ¸Ğ°Ñ‚ÑƒÑ€Ğ¾Ğ¹
                await self._send_safe_message(
                    chat_id=message.chat.id,
                    text=(
                        f"{status_line_admin}\n"
                        f"{self.texts.menu_you_are_admin}\n"
                        f"{self.texts.admin_panel_choose_section}"
                    ),
                    reply_markup=await self.get_admin_menu(),
                )
        else:
            # Ğ’ĞµÑ‚ĞºĞ° Ğ´Ğ»Ñ Ğ¾Ğ±Ñ‹Ñ‡Ğ½Ğ¾Ğ³Ğ¾ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ

            # ĞŸÑ€Ğ¾Ğ²ĞµÑ€ĞºĞ° Ğ½Ğ° blacklist
            if await self.is_user_blacklisted(user_id):
                await self._send_safe_message(
                    chat_id=message.chat.id,
                    text=self.texts.you_are_blocked,
                )
                return

            # ĞŸÑ‹Ñ‚Ğ°ĞµĞ¼ÑÑ Ğ²Ğ·ÑÑ‚ÑŒ ĞºĞ°ÑÑ‚Ğ¾Ğ¼Ğ½Ğ¾Ğµ Ğ¿Ñ€Ğ¸Ğ²ĞµÑ‚ÑÑ‚Ğ²Ğ¸Ğµ Ğ¸Ğ· Ğ½Ğ°ÑÑ‚Ñ€Ğ¾ĞµĞº
            greeting = await self.get_setting("greeting_text")
            if not greeting or not greeting.strip():
                # Ğ•ÑĞ»Ğ¸ Ğ°Ğ´Ğ¼Ğ¸Ğ½ ĞµÑ‰Ñ‘ Ğ½Ğµ Ğ·Ğ°Ğ´Ğ°Ğ» Ğ¿Ñ€Ğ¸Ğ²ĞµÑ‚ÑÑ‚Ğ²Ğ¸Ğµ â€” Ğ¸ÑĞ¿Ğ¾Ğ»ÑŒĞ·ÑƒĞµĞ¼ Ğ´ĞµÑ„Ğ¾Ğ»Ñ‚Ğ½Ğ¾Ğµ Ğ¸Ğ· ÑĞ·Ñ‹ĞºĞ¾Ğ²Ñ‹Ñ… Ñ‚ĞµĞºÑÑ‚Ğ¾Ğ²
                # (ÑƒĞ±ĞµĞ´Ğ¸ÑÑŒ, Ñ‡Ñ‚Ğ¾ Ñ‚Ğ°ĞºĞ¾Ğµ Ğ¿Ğ¾Ğ»Ğµ ĞµÑÑ‚ÑŒ Ğ² self.texts, Ğ»Ğ¸Ğ±Ğ¾ Ğ¿Ğ¾Ğ¼ĞµĞ½ÑĞ¹ Ğ¸Ğ¼Ñ)
                greeting = self.texts.default_greeting

            # ĞÑ‚Ğ¿Ñ€Ğ°Ğ²Ğ»ÑĞµĞ¼ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ Ğ¿Ñ€Ğ¸Ğ²ĞµÑ‚ÑÑ‚Ğ²Ğ¸Ğµ
            await self._send_safe_message(
                chat_id=message.chat.id,
                text=greeting,
            )

    async def cmd_admin(self, message: Message, state: FSMContext) -> None:
        if not await self.is_admin(message.from_user.id):
            await self._send_safe_message(
                chat_id=message.chat.id,
                text=self.texts.access_denied,
            )
            return

        await state.clear()
        await self._send_safe_message(
            chat_id=message.chat.id,
            text=self.texts.admin_panel_title,
            reply_markup=await self.get_admin_menu(),
        )


    async def cmd_openchat_off(self, message: Message, state: FSMContext) -> None:
        """
        ĞÑ‚ĞºĞ»ÑÑ‡ĞµĞ½Ğ¸Ğµ OpenChat Ğ¸Ğ· Ğ¿Ñ€Ğ¸Ğ²Ğ°Ñ‚Ğ½Ğ¾Ğ³Ğ¾ Ñ‡Ğ°Ñ‚Ğ°.
        """
        if not await self.is_admin(message.from_user.id):
            await self._send_safe_message(
                chat_id=message.chat.id,
                text=self.texts.access_denied,
            )
            return

        # Ğ“Ğ°ÑĞ¸Ğ¼ Ğ² worker settings
        await self.set_setting("openchat_enabled", "False")
        await self.set_setting("general_panel_chat_id", "")
        await self.set_setting("openchat_username", "")

        try:
            await self.db.execute(
                """
                INSERT INTO instance_meta (
                    instance_id,
                    openchat_username,
                    general_panel_chat_id,
                    openchat_enabled,
                    updated_at
                )
                VALUES (%s, NULL, NULL, %s, NOW())
                ON CONFLICT (instance_id) DO UPDATE
                SET openchat_username     = NULL,
                    general_panel_chat_id = NULL,
                    openchat_enabled      = EXCLUDED.openchat_enabled,
                    updated_at            = NOW()
                """,
                (
                    self.instance_id,
                    False,
                ),
            )
        except Exception as e:
            logger.error(f"Failed to update instance_meta (openchat off) for {self.instance_id}: {e}")

        await self._send_safe_message(
            chat_id=message.chat.id,
            text=self.texts.openchat_off_confirm,
        )

    async def cmd_bind_openchat(self, message: Message, state: FSMContext) -> None:
        """
        ĞŸÑ€Ğ¸Ğ²ÑĞ·ĞºĞ° OpenChat Ğ¸Ğ· ÑĞ°Ğ¼Ğ¾Ğ³Ğ¾ Ğ³Ñ€ÑƒĞ¿Ğ¿Ğ¾Ğ²Ğ¾Ğ³Ğ¾ Ñ‡Ğ°Ñ‚Ğ°:
        /bind @bot_name_bot
        """
        user_id = message.from_user.id

        # Ğ¢Ğ¾Ğ»ÑŒĞºĞ¾ Ğ°Ğ´Ğ¼Ğ¸Ğ½ Ğ¸Ğ½ÑÑ‚Ğ°Ğ½ÑĞ° Ğ¼Ğ¾Ğ¶ĞµÑ‚ Ğ¿Ñ€Ğ¸Ğ²ÑĞ·Ñ‹Ğ²Ğ°Ñ‚ÑŒ OpenChat
        if not await self.is_admin(user_id):
            await self._send_safe_message(
                chat_id=message.chat.id,
                text=self.texts.openchat_bind_only_owner,
            )
            return

        parts = (message.text or "").split()
        if len(parts) > 1:
            arg = parts[1].lstrip("@")
            me = await self.bot.get_me()
            if arg.lower() != (me.username or "").lower():
                await self._send_safe_message(
                    chat_id=message.chat.id,
                    text=self.texts.openchat_bind_usage_error,
                )
                return

        chat = message.chat

        # 1. Ğ§Ğ°Ñ‚ Ğ´Ğ¾Ğ»Ğ¶ĞµĞ½ Ğ±Ñ‹Ñ‚ÑŒ ÑÑƒĞ¿ĞµÑ€Ğ³Ñ€ÑƒĞ¿Ğ¿Ğ¾Ğ¹
        if chat.type != ChatType.SUPERGROUP:
            await self._send_safe_message(
                chat_id=message.chat.id,
                text=self.texts.openchat_not_supergroup,
            )
            return

        # 2. ĞĞµ Ğ´Ğ¾Ğ»Ğ¶ĞµĞ½ Ğ¸Ğ¼ĞµÑ‚ÑŒ username
        if chat.username:
            await self._send_safe_message(
                chat_id=message.chat.id,
                text=self.texts.openchat_has_username.format(
                    chat_username=chat.username
                ),
            )
            return

        # 3. Ğ”Ğ¾Ğ»Ğ¶ĞµĞ½ Ğ±Ñ‹Ñ‚ÑŒ Ğ²ĞºĞ»ÑÑ‡ĞµĞ½ Ñ„Ğ¾Ñ€ÑƒĞ¼Ğ½Ñ‹Ğ¹ Ñ€ĞµĞ¶Ğ¸Ğ¼ (topics)
        if not chat.is_forum:
            await self._send_safe_message(
                chat_id=message.chat.id,
                text=self.texts.openchat_no_forum,
            )
            return

        # Ğ¡Ğ¾Ñ…Ñ€Ğ°Ğ½ÑĞµĞ¼ Ğ² worker settings (ĞºĞ°Ğº Ğ±Ñ‹Ğ»Ğ¾)
        await self.set_setting("openchat_enabled", "True")
        await self.set_setting("general_panel_chat_id", str(chat.id))
        await self.set_setting("openchat_username", chat.username or "")

        # Ğ”Ğ¾Ğ¿Ğ¾Ğ»Ğ½Ğ¸Ñ‚ĞµĞ»ÑŒĞ½Ğ¾ ÑĞ¸Ğ½Ñ…Ñ€Ğ¾Ğ½Ğ¸Ğ·Ğ¸Ñ€ÑƒĞµĞ¼ Ğ² master Postgres Ğ´Ğ»Ñ mini-app (instance_meta)
        try:
            await self.db.execute(
                """
                INSERT INTO instance_meta (
                    instance_id,
                    openchat_username,
                    general_panel_chat_id,
                    openchat_enabled,
                    updated_at
                )
                VALUES (%s, %s, %s, %s, NOW())
                ON CONFLICT (instance_id) DO UPDATE
                  SET openchat_username     = EXCLUDED.openchat_username,
                      general_panel_chat_id = EXCLUDED.general_panel_chat_id,
                      openchat_enabled      = EXCLUDED.openchat_enabled,
                      updated_at            = NOW()
                """,
                (
                    self.instance_id,           # Ğ¸Ğ»Ğ¸ self.instanceid, ĞºĞ°Ğº Ñƒ Ñ‚ĞµĞ±Ñ Ñ€ĞµĞ°Ğ»ÑŒĞ½Ğ¾ Ğ½Ğ°Ğ·Ñ‹Ğ²Ğ°ĞµÑ‚ÑÑ
                    chat.username or None,
                    chat.id,
                    True,
                ),
            )
        except Exception as e:
            logger.error(
                f"Failed to upsert instance_meta for instance {self.instance_id}: {e}"
            )

        await self._send_safe_message(
            chat_id=message.chat.id,
            text=self.texts.openchat_bound_ok.format(chat_title=chat.title),
        )

    # ====================== CALLBACKS (Ğ¢Ğ˜ĞšĞ•Ğ¢Ğ«) ======================

    async def handle_ticket_callback(self, cb: CallbackQuery) -> None:
        """
        ĞĞ±Ñ€Ğ°Ğ±Ğ¾Ñ‚ĞºĞ° callback'Ğ¾Ğ² Ğ¾Ñ‚ ĞºĞ»Ğ°Ğ²Ğ¸Ğ°Ñ‚ÑƒÑ€Ñ‹ Ñ‚Ğ¸ĞºĞµÑ‚Ğ¾Ğ²:
        Ğ¼ĞµĞ½Ñ / Ğ¡ĞµĞ±Ğµ / ĞĞ°Ğ·Ğ½Ğ°Ñ‡Ğ¸Ñ‚ÑŒ / Ğ¡Ğ¿Ğ°Ğ¼ / ĞĞµ ÑĞ¿Ğ°Ğ¼ / Ğ—Ğ°ĞºÑ€Ñ‹Ñ‚ÑŒ / ĞŸĞµÑ€ĞµĞ¾Ñ‚ĞºÑ€Ñ‹Ñ‚ÑŒ.
        """
        data = cb.data or ""
        if not data.startswith("ticket:"):
            return

        parts = data.split(":")
        if len(parts) < 3:
            await cb.answer()
            return

        action = parts[1]

        # ĞÑ‚ĞºÑ€Ñ‹Ñ‚ÑŒ Ğ¿Ğ¾Ğ»Ğ½Ğ¾Ğµ Ğ¼ĞµĞ½Ñ Ğ¸Ğ· ĞºĞ¾Ğ¼Ğ¿Ğ°ĞºÑ‚Ğ½Ğ¾Ğ¹ ĞºĞ½Ğ¾Ğ¿ĞºĞ¸ ğŸ–²
        if action == "menu":
            try:
                ticket_id = int(parts[2])
            except ValueError:
                await cb.answer()
                return

            ticket = await self.fetch_ticket(ticket_id)
            if not ticket:
                await cb.answer("Ğ¢Ğ¸ĞºĞµÑ‚ Ğ½Ğµ Ğ½Ğ°Ğ¹Ğ´ĞµĞ½", show_alert=True)
                return

            status = ticket.get("status") or "new"
            can_close = status not in ("closed", "spam")
            is_spam = status == "spam"
            is_closed = status == "closed"

            kb = self._build_full_ticket_keyboard(
                ticket_id,
                can_close,
                is_spam=is_spam,
                is_closed=is_closed,
            )
            if cb.message:
                await cb.message.edit_reply_markup(reply_markup=kb)

            await cb.answer()
            return

        # Ğ¡Ğ²ĞµÑ€Ğ½ÑƒÑ‚ÑŒ Ğ¿Ğ¾Ğ»Ğ½Ğ¾Ğµ Ğ¼ĞµĞ½Ñ Ğ¾Ğ±Ñ€Ğ°Ñ‚Ğ½Ğ¾ Ğ² Ğ¾Ğ´Ğ½Ñƒ ĞºĞ½Ğ¾Ğ¿ĞºÑƒ ğŸ–²
        if action == "compact":
            try:
                ticket_id = int(parts[2])
            except ValueError:
                await cb.answer()
                return

            if cb.message:
                await self.put_ticket_keyboard(
                    ticket_id,
                    cb.message.message_id,
                    compact=True,
                )

            await cb.answer()
            return

        # ĞĞ¸Ğ¶Ğµ â€” Ğ´ĞµĞ¹ÑÑ‚Ğ²Ğ¸Ñ, Ñ‚Ñ€ĞµĞ±ÑƒÑÑ‰Ğ¸Ğµ ticket_id Ğ¸ ÑĞ°Ğ¼Ğ¾Ğ³Ğ¾ Ñ‚Ğ¸ĞºĞµÑ‚Ğ°
        try:
            ticket_id = int(parts[2])
        except ValueError:
            await cb.answer()
            return

        ticket = await self.fetch_ticket(ticket_id)
        if not ticket:
            await cb.answer("Ğ¢Ğ¸ĞºĞµÑ‚ Ğ½Ğµ Ğ½Ğ°Ğ¹Ğ´ĞµĞ½", show_alert=True)
            return

        message = cb.message
        if not message:
            await cb.answer()
            return

        user = cb.from_user
        assignee_username = user.username or f"id{user.id}"

        # 1) "Ğ¡ĞµĞ±Ğµ"
        if action == "self":
            current_status = ticket.get("status") or "new"
            new_status = current_status
            if current_status == "new":
                new_status = "inprogress"

            await self.set_ticket_status(
                ticket_id,
                new_status,
                assigned_username=assignee_username,
                assigned_user_id=user.id,
            )
            # Ğ¿Ğ¾ÑĞ»Ğµ Ğ´ĞµĞ¹ÑÑ‚Ğ²Ğ¸Ñ ÑĞ²Ğ¾Ñ€Ğ°Ñ‡Ğ¸Ğ²Ğ°ĞµĞ¼ Ğ¼ĞµĞ½Ñ
            await self.put_ticket_keyboard(ticket_id, message.message_id, compact=True)
            await cb.answer("Ğ¢Ğ¸ĞºĞµÑ‚ Ğ²Ğ·ÑÑ‚ Ğ² Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñƒ")
            return

        # 2) "ĞĞ°Ğ·Ğ½Ğ°Ñ‡Ğ¸Ñ‚ÑŒ" â€” Ğ¿Ğ¾ĞºĞ°Ğ·Ğ°Ñ‚ÑŒ ÑĞ¿Ğ¸ÑĞ¾Ğº Ğ´Ñ€ÑƒĞ³Ğ¸Ñ… ÑƒÑ‡Ğ°ÑÑ‚Ğ½Ğ¸ĞºĞ¾Ğ² (Ğ°Ğ´Ğ¼Ğ¸Ğ½Ğ¾Ğ²) Ñ‡Ğ°Ñ‚Ğ°
        if action == "assign":
            members = await self.bot.get_chat_administrators(ticket["chat_id"])
            rows: List[List[InlineKeyboardButton]] = []

            for m in members:
                if m.user.is_bot or m.user.id == user.id:
                    continue
                label = (
                    f"@{m.user.username}"
                    if m.user.username
                    else m.user.full_name
                )
                rows.append(
                    [
                        InlineKeyboardButton(
                            text=label,
                            callback_data=f"ticket:assign_to:{ticket_id}:{m.user.id}",
                        )
                    ]
                )
            if not rows:
                await cb.answer("ĞĞµĞºĞ¾Ğ³Ğ¾ Ğ½Ğ°Ğ·Ğ½Ğ°Ñ‡Ğ°Ñ‚ÑŒ", show_alert=True)
                return

            rows.append(
                [
                    InlineKeyboardButton(
                        text="ĞÑ‚Ğ¼ĞµĞ½Ğ°",
                        callback_data=f"ticket:cancel_assign:{ticket_id}",
                    )
                ]
            )

            await message.edit_reply_markup(
                reply_markup=InlineKeyboardMarkup(inline_keyboard=rows)
            )
            await cb.answer()
            return

        # 2a) Ğ’Ñ‹Ğ±Ğ¾Ñ€ ĞºĞ¾Ğ½ĞºÑ€ĞµÑ‚Ğ½Ğ¾Ğ³Ğ¾ Ğ¸ÑĞ¿Ğ¾Ğ»Ğ½Ğ¸Ñ‚ĞµĞ»Ñ
        if action == "assign_to" and len(parts) == 4:
            try:
                assignee_id = int(parts[3])
            except ValueError:
                await cb.answer()
                return

            member = await self.bot.get_chat_member(ticket["chat_id"], assignee_id)
            target_username = member.user.username or f"id{member.user.id}"

            current_status = ticket.get("status") or "new"
            new_status = current_status
            if current_status == "new":
                new_status = "inprogress"

            await self.set_ticket_status(
                ticket_id,
                new_status,
                assigned_username=target_username,
                assigned_user_id=assignee_id,
            )
            await self.put_ticket_keyboard(ticket_id, message.message_id, compact=True)
            await cb.answer(f"ĞĞ°Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ¾ {target_username}")
            return

        # 2b) ĞÑ‚Ğ¼ĞµĞ½Ğ° Ğ½Ğ°Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ¸Ñ
        if action == "cancel_assign":
            await self.put_ticket_keyboard(ticket_id, message.message_id, compact=True)
            await cb.answer("ĞÑ‚Ğ¼ĞµĞ½ĞµĞ½Ğ¾")
            return

        # 3) "Ğ¡Ğ¿Ğ°Ğ¼" â€” Ğ¿Ğ¾Ğ¼ĞµÑ‚Ğ¸Ñ‚ÑŒ Ñ‚Ğ¸ĞºĞµÑ‚ ĞºĞ°Ğº ÑĞ¿Ğ°Ğ¼,
        # ÑƒĞ±Ñ€Ğ°Ñ‚ÑŒ Ğ²Ğ¾Ğ·Ğ¼Ğ¾Ğ¶Ğ½Ğ¾ÑÑ‚ÑŒ "Ğ¡ĞµĞ±Ğµ"/"ĞĞ°Ğ·Ğ½Ğ°Ñ‡Ğ¸Ñ‚ÑŒ", Ğ¿Ğ¾ĞºĞ°Ğ·Ğ°Ñ‚ÑŒ "ĞĞµ ÑĞ¿Ğ°Ğ¼"
        if action == "spam":
            await self.set_ticket_status(
                ticket_id,
                "spam",
                assigned_username=None,
                assigned_user_id=None,
            )
            await self.put_ticket_keyboard(ticket_id, message.message_id, compact=True)
            await cb.answer("ĞÑ‚Ğ¼ĞµÑ‡ĞµĞ½Ğ¾ ĞºĞ°Ğº ÑĞ¿Ğ°Ğ¼")
            return

        # 3a) "ĞĞµ ÑĞ¿Ğ°Ğ¼" â€” Ğ²ĞµÑ€Ğ½ÑƒÑ‚ÑŒ Ñ‚Ğ¸ĞºĞµÑ‚ Ğ¸Ğ· ÑĞ¿Ğ°Ğ¼Ğ° Ğ² Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñƒ Ñ Ñ‚ĞµĞºÑƒÑ‰Ğ¸Ğ¼ Ğ°Ğ´Ğ¼Ğ¸Ğ½Ğ¾Ğ¼
        if action == "not_spam":
            await self.set_ticket_status(
                ticket_id,
                "inprogress",
                assigned_username=assignee_username,
                assigned_user_id=user.id,
            )
            await self.put_ticket_keyboard(ticket_id, message.message_id, compact=True)
            await cb.answer("Ğ¢Ğ¸ĞºĞµÑ‚ Ğ²Ğ¾Ğ·Ğ²Ñ€Ğ°Ñ‰Ñ‘Ğ½ Ğ¸Ğ· ÑĞ¿Ğ°Ğ¼Ğ°")
            return

        # 4) "Ğ—Ğ°ĞºÑ€Ñ‹Ñ‚ÑŒ"
        if action == "close":
            await self.set_ticket_status(
                ticket_id,
                "closed",
                assigned_username=ticket.get("assigned_username"),
                assigned_user_id=ticket.get("assigned_user_id"),
            )
            await self.put_ticket_keyboard(ticket_id, message.message_id, compact=True)
            await cb.answer("Ğ¢Ğ¸ĞºĞµÑ‚ Ğ·Ğ°ĞºÑ€Ñ‹Ñ‚")
            return

        # 4a) "ĞŸĞµÑ€ĞµĞ¾Ñ‚ĞºÑ€Ñ‹Ñ‚ÑŒ"
        if action == "reopen":
            await self.set_ticket_status(
                ticket_id,
                "inprogress",
                assigned_username=assignee_username,
                assigned_user_id=user.id,
            )
            await self.put_ticket_keyboard(ticket_id, message.message_id, compact=True)
            await cb.answer("Ğ¢Ğ¸ĞºĞµÑ‚ Ğ¿ĞµÑ€ĞµĞ¾Ñ‚ĞºÑ€Ñ‹Ñ‚")
            return

        await cb.answer()


    # ====================== CALLBACKS ĞĞ”ĞœĞ˜Ğ-ĞŸĞĞĞ•Ğ›Ğ˜ ======================

    async def handle_callback(self, cb: CallbackQuery, state: FSMContext) -> None:
        if not await self.is_admin(cb.from_user.id):
            await cb.answer(self.texts.access_denied, show_alert=True)
            return

        data = cb.data or ""

        if data == "edit_greeting":
            await state.set_state(AdminStates.wait_greeting)
            await cb.message.edit_text(
                self.texts.greeting_edit_prompt,
                reply_markup=InlineKeyboardMarkup(
                    inline_keyboard=[
                        [
                            InlineKeyboardButton(
                                text=self.texts.back,
                                callback_data="main_menu",
                            )
                        ]
                    ]
                ),
            )

        elif data == "setup_autoreply":
            await state.set_state(AdminStates.wait_autoreply)
            enabled = (
                self.texts.autoreply_state_on.format(
                    state=self.texts.autoreply_enabled_label
                    if await self.get_setting("autoreply_enabled") == "True"
                    else self.texts.autoreply_disabled_label
                )
            )
            await cb.message.edit_text(
                enabled,
                reply_markup=InlineKeyboardMarkup(
                    inline_keyboard=[
                        [
                            InlineKeyboardButton(
                                text=self.texts.back,
                                callback_data="main_menu",
                            )
                        ]
                    ]
                ),
            )

        elif data == "setup_openchat":
            openchat = await self.get_openchat_settings()
            status = self.texts.openchat_status_on if openchat["enabled"] else self.texts.openchat_status_off

            if openchat["chat_id"]:
                current = self.texts.openchat_current_chat_id.format(
                    chat_id=openchat["chat_id"]
                )
            else:
                current = self.texts.openchat_not_bound

            me = await self.bot.get_me()
            bot_username = me.username or "bot"
            await cb.message.edit_text(
                self.texts.openchat_now_status.format(
                    status=status,
                    current=current,
                    bot_username=bot_username,
                ),
                reply_markup=InlineKeyboardMarkup(
                    inline_keyboard=[
                        [
                            InlineKeyboardButton(
                                text=self.texts.back,
                                callback_data="main_menu",
                            )
                        ]
                    ]
                ),
            )

        elif data == "setup_privacy":
            enabled = self.texts.privacy_state_on if await self.is_privacy_enabled() else self.texts.privacy_state_off
            await cb.message.edit_text(
                self.texts.privacy_screen.format(state=enabled),
                reply_markup=InlineKeyboardMarkup(
                    inline_keyboard=[
                        [
                            InlineKeyboardButton(
                                text=self.texts.privacy_toggle_btn,
                                callback_data="toggle_privacy",
                            )
                        ],
                        [
                            InlineKeyboardButton(
                                text=self.texts.back,
                                callback_data="main_menu",
                            )
                        ],
                    ]
                ),
            )

        elif data == "toggle_privacy":
            current = await self.is_privacy_enabled()
            await self.set_setting("privacy_mode_enabled", "False" if current else "True")
            new_state = (
                self.texts.privacy_state_on if not current else self.texts.privacy_state_off
            )
            await cb.answer(
                self.texts.privacy_toggled.format(state=new_state),
                show_alert=False,
            )

            enabled = (
                self.texts.privacy_state_on if await self.is_privacy_enabled() else self.texts.privacy_state_off
            )
            await cb.message.edit_text(
                self.texts.privacy_screen.format(state=enabled),
                reply_markup=InlineKeyboardMarkup(
                    inline_keyboard=[
                        [
                            InlineKeyboardButton(
                                text=self.texts.privacy_toggle_btn,
                                callback_data="toggle_privacy",
                            )
                        ],
                        [
                            InlineKeyboardButton(
                                text=self.texts.back,
                                callback_data="main_menu",
                            )
                        ],
                    ]
                ),
            )

        elif data == "blacklist":
            await state.set_state(AdminStates.wait_blacklist_menu)
            await cb.message.edit_text(
                self.texts.blacklist_title,
                reply_markup=self.get_blacklist_menu(),
            )

        elif data == "blacklist_add":
            await state.set_state(AdminStates.wait_blacklist_add)
            await cb.message.edit_text(
                self.texts.blacklist_add_prompt,
                reply_markup=InlineKeyboardMarkup(
                    inline_keyboard=[
                        [
                            InlineKeyboardButton(
                                text=self.texts.back,
                                callback_data="blacklist",
                            )
                        ]
                    ]
                ),
            )

        elif data == "blacklist_remove":
            await state.set_state(AdminStates.wait_blacklist_remove)
            await cb.message.edit_text(
                self.texts.blacklist_remove_prompt,
                reply_markup=InlineKeyboardMarkup(
                    inline_keyboard=[
                        [
                            InlineKeyboardButton(
                                text=self.texts.back,
                                callback_data="blacklist",
                            )
                        ]
                    ]
                ),
            )

        elif data == "blacklist_show":
            await self.render_blacklist_page(cb, page=0)

        elif data.startswith("bl_page:"):
            try:
                page = int(data.split(":", 1)[1])
            except ValueError:
                await cb.answer()
                return
            await self.render_blacklist_page(cb, page=page)

        elif data == "blacklist_search":
            await state.set_state(AdminStates.wait_blacklist_search)
            await cb.message.edit_text(
                self.texts.blacklist_search_prompt,
                reply_markup=InlineKeyboardMarkup(
                    inline_keyboard=[
                        [
                            InlineKeyboardButton(
                                text=self.texts.back,
                                callback_data="blacklist_show",
                            )
                        ]
                    ]
                ),
            )

        elif data == "export_users":
            await cb.answer(self.texts.export_preparing, show_alert=False)

            rows = await self.db.fetchall(
                """
                SELECT DISTINCT user_id, username, created_at
                FROM tickets
                WHERE instance_id = %s
                ORDER BY created_at ASC
                """,
                (self.instance_id,),
            )

            if not rows:
                await cb.message.answer(self.texts.export_no_users)
                return

            import io
            import csv
            from datetime import datetime as _dt
            from aiogram.types import BufferedInputFile

            buf = io.StringIO()
            writer = csv.writer(buf)
            writer.writerow(["user_id", "username", "first_seen"])
            for r in rows:
                created = r["created_at"]
                if isinstance(created, _dt):
                    created = created.isoformat()
                writer.writerow([r["user_id"], r["username"] or "", created])

            data_bytes = buf.getvalue().encode("utf-8")
            file = BufferedInputFile(file=data_bytes, filename="users_export.csv")

            await cb.message.answer_document(
                document=file,
                caption=self.texts.export_users_caption,
            )

        elif data == "main_menu":
            await state.clear()

            openchat = await self.get_openchat_settings()
            if openchat["enabled"] and openchat["chat_id"]:
                status_line_admin = self.texts.openchat_status_line_on
            else:
                status_line_admin = self.texts.openchat_status_line_off

            me = await self.bot.get_me()
            bot_username = me.username or "bot"

            if not openchat["enabled"]:
                text = (
                    f"{status_line_admin}\n"
                    f"{self.texts.menu_you_are_admin}\n\n"
                    + self.texts.openchat_setup_hint.format(bot_username=bot_username)
                )
                reply_markup = InlineKeyboardMarkup(
                    inline_keyboard=[
                        [
                            InlineKeyboardButton(
                                text=self.texts.openchat_setup_button,
                                callback_data="setup_openchat",
                            )
                        ]
                    ]
                )
            else:
                text = (
                    f"{status_line_admin}\n"
                    f"{self.texts.menu_you_are_admin}\n"
                    f"{self.texts.admin_panel_choose_section}"
                )
                reply_markup = await self.get_admin_menu()

            await cb.message.edit_text(text, reply_markup=reply_markup)

        else:
            await cb.answer()


    # ====================== ĞĞ‘Ğ ĞĞ‘ĞĞ¢ĞšĞ Ğ¡ĞĞ¡Ğ¢ĞĞ¯ĞĞ˜Ğ™ ĞĞ”ĞœĞ˜ĞĞ ======================

    async def handle_admin_blacklist_search(self, message: Message, state: FSMContext) -> None:
        if not await self.is_admin(message.from_user.id):
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="âŒ Ğ”Ğ¾ÑÑ‚ÑƒĞ¿ Ğ·Ğ°Ğ¿Ñ€ĞµÑ‰Ñ‘Ğ½",
            )
            return

        if not message.text:
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="Ğ¢Ñ€ĞµĞ±ÑƒĞµÑ‚ÑÑ Ñ‚ĞµĞºÑÑ‚ Ğ´Ğ»Ñ Ğ¿Ğ¾Ğ¸ÑĞºĞ°.",
            )
            return

        query = message.text.strip().lstrip("@").lower()

        bl = await self.get_blacklist()
        results = [
            u
            for u in bl
            if (u["username"] or "").lower().find(query) != -1
        ]

        if not results:
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="ĞĞ¸Ñ‡ĞµĞ³Ğ¾ Ğ½Ğµ Ğ½Ğ°Ğ¹Ğ´ĞµĞ½Ğ¾ Ğ² Ñ‡Ñ‘Ñ€Ğ½Ğ¾Ğ¼ ÑĞ¿Ğ¸ÑĞºĞµ.",
            )
            return

        lines = []
        for u in results[:50]:
            label = f"@{u['username']}" if u["username"] else ""
            lines.append(f"<code>{u['user_id']}</code> {label}")

        text = (
            f"ğŸ” Ğ ĞµĞ·ÑƒĞ»ÑŒÑ‚Ğ°Ñ‚Ñ‹ Ğ¿Ğ¾Ğ¸ÑĞºĞ° Ğ¿Ğ¾ \"{query}\":\n"
            + "\n".join(lines)
        )
        if len(results) > 50:
            text += f"\n\nĞŸĞ¾ĞºĞ°Ğ·Ğ°Ğ½Ñ‹ Ğ¿ĞµÑ€Ğ²Ñ‹Ğµ 50 Ğ¸Ğ· {len(results)} Ğ·Ğ°Ğ¿Ğ¸ÑĞµĞ¹."

        await state.set_state(AdminStates.wait_blacklist_menu)
        await self._send_safe_message(
            chat_id=message.chat.id,
            text=text,
            reply_markup=self.get_blacklist_menu(),
        )


    async def handle_admin_greeting(self, message: Message, state: FSMContext) -> None:
        if not await self.is_admin(message.from_user.id):
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="âŒ Ğ”Ğ¾ÑÑ‚ÑƒĞ¿ Ğ·Ğ°Ğ¿Ñ€ĞµÑ‰Ñ‘Ğ½",
            )
            return

        if message.text and message.text.strip() == "/clear_greeting":
            await self.set_setting("greeting_text", "")
            await state.clear()
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="âœ… ĞŸÑ€Ğ¸Ğ²ĞµÑ‚ÑÑ‚Ğ²Ğ¸Ğµ ÑƒĞ´Ğ°Ğ»ĞµĞ½Ğ¾.",
            )
            return

        if not message.text:
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="Ğ¢Ñ€ĞµĞ±ÑƒĞµÑ‚ÑÑ Ñ‚ĞµĞºÑÑ‚Ğ¾Ğ²Ğ¾Ğµ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğµ Ñ Ğ¿Ñ€Ğ¸Ğ²ĞµÑ‚ÑÑ‚Ğ²Ğ¸ĞµĞ¼.",
            )
            return

        await self.set_setting("greeting_text", message.text)
        await state.clear()
        await self._send_safe_message(
            chat_id=message.chat.id,
            text="âœ… ĞĞ¾Ğ²Ğ¾Ğµ Ğ¿Ñ€Ğ¸Ğ²ĞµÑ‚ÑÑ‚Ğ²Ğ¸Ğµ ÑĞ¾Ñ…Ñ€Ğ°Ğ½ĞµĞ½Ğ¾.",
        )

    async def handle_admin_autoreply(self, message: Message, state: FSMContext) -> None:
        if not await self.is_admin(message.from_user.id):
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="âŒ Ğ”Ğ¾ÑÑ‚ÑƒĞ¿ Ğ·Ğ°Ğ¿Ñ€ĞµÑ‰Ñ‘Ğ½",
            )
            return

        if message.text and message.text.strip() == "/autoreply_off":
            awaitself.set_setting("autoreply_enabled", "False")
            await self.set_setting("autoreply_text", "")
            await state.clear()
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="âœ… ĞĞ²Ñ‚Ğ¾Ğ¾Ñ‚Ğ²ĞµÑ‚Ñ‹ Ğ¾Ñ‚ĞºĞ»ÑÑ‡ĞµĞ½Ñ‹.",
            )
            return

        if not message.text:
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="ĞÑ‚Ğ¿Ñ€Ğ°Ğ²ÑŒÑ‚Ğµ Ñ‚ĞµĞºÑÑ‚ Ğ°Ğ²Ñ‚Ğ¾Ğ¾Ñ‚Ğ²ĞµÑ‚Ğ° Ğ¸Ğ»Ğ¸ /autoreply_off.",
            )
            return

        await self.set_setting("autoreply_enabled", "True")
        await self.set_setting("autoreply_text", message.text)
        await state.clear()
        await self._send_safe_message(
            chat_id=message.chat.id,
            text="âœ… ĞĞ²Ñ‚Ğ¾Ğ¾Ñ‚Ğ²ĞµÑ‚ ÑĞ¾Ñ…Ñ€Ğ°Ğ½Ñ‘Ğ½ Ğ¸ Ğ²ĞºĞ»ÑÑ‡Ñ‘Ğ½.",
        )

    async def handle_admin_blacklist_add(self, message: Message, state: FSMContext) -> None:
        if not await self.is_admin(message.from_user.id):
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="âŒ Ğ”Ğ¾ÑÑ‚ÑƒĞ¿ Ğ·Ğ°Ğ¿Ñ€ĞµÑ‰Ñ‘Ğ½",
            )
            return

        if not message.text:
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="Ğ¢Ñ€ĞµĞ±ÑƒĞµÑ‚ÑÑ ID Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ Ğ¸Ğ»Ğ¸ username.",
            )
            return

        parts = message.text.strip().split()

        user_id: Optional[int] = None
        username: str = ""

        # Ğ’Ğ°Ñ€Ğ¸Ğ°Ğ½Ñ‚ 1: ÑƒĞºĞ°Ğ·Ğ°Ğ½ Ñ‡Ğ¸ÑĞ»Ğ¾Ğ²Ğ¾Ğ¹ ID (Ñ Ğ¾Ğ¿Ñ†Ğ¸Ğ¾Ğ½Ğ°Ğ»ÑŒĞ½Ñ‹Ğ¼ username Ğ²Ñ‚Ğ¾Ñ€Ñ‹Ğ¼ Ğ°Ñ€Ğ³ÑƒĞ¼ĞµĞ½Ñ‚Ğ¾Ğ¼)
        try:
            user_id = int(parts[0])
            if len(parts) > 1:
                username = parts[1].lstrip("@")
        except ValueError:
            # Ğ’Ğ°Ñ€Ğ¸Ğ°Ğ½Ñ‚ 2: Ğ½ĞµÑ‚ ID, Ñ‚Ğ¾Ğ»ÑŒĞºĞ¾ username
            username = parts[0].lstrip("@")

        if user_id is None:
            # ĞŸÑ‹Ñ‚Ğ°ĞµĞ¼ÑÑ Ğ½Ğ°Ğ¹Ñ‚Ğ¸ user_id Ğ¿Ğ¾ username Ğ² Ñ‚Ğ°Ğ±Ğ»Ğ¸Ñ†Ğµ tickets
            row = await self.db.fetchone(
                """
                SELECT DISTINCT user_id
                FROM tickets
                WHERE instance_id = %s AND username = %s
                ORDER BY created_at DESC
                LIMIT 1
                """,
                (self.instance_id, username),
            )
            if not row:
                await self._send_safe_message(
                    chat_id=message.chat.id,
                    text=f"ĞĞµ ÑƒĞ´Ğ°Ğ»Ğ¾ÑÑŒ Ğ½Ğ°Ğ¹Ñ‚Ğ¸ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ Ñ username @{username} "
                         f"ÑÑ€ĞµĞ´Ğ¸ Ñ‚Ğ¸ĞºĞµÑ‚Ğ¾Ğ². Ğ£ĞºĞ°Ğ¶Ğ¸ numeric ID Ğ¸Ğ»Ğ¸ Ğ²ĞµÑ€Ğ½Ñ‹Ğ¹ username.",
                )
                return
            user_id = row["user_id"]

        await self.add_to_blacklist(user_id, username)

        await state.set_state(AdminStates.wait_blacklist_menu)
        await self._send_safe_message(
            chat_id=message.chat.id,
            text=f"âœ… ĞŸĞ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»ÑŒ <code>{user_id}</code> Ğ´Ğ¾Ğ±Ğ°Ğ²Ğ»ĞµĞ½ Ğ² Ñ‡Ñ‘Ñ€Ğ½Ñ‹Ğ¹ ÑĞ¿Ğ¸ÑĞ¾Ğº.",
            reply_markup=self.get_blacklist_menu(),
        )


    async def handle_admin_blacklist_remove(self, message: Message, state: FSMContext) -> None:
        if not await self.is_admin(message.from_user.id):
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="âŒ Ğ”Ğ¾ÑÑ‚ÑƒĞ¿ Ğ·Ğ°Ğ¿Ñ€ĞµÑ‰Ñ‘Ğ½",
            )
            return

        if not message.text:
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="ĞÑ‚Ğ¿Ñ€Ğ°Ğ²ÑŒÑ‚Ğµ ID Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ Ğ´Ğ»Ñ ÑƒĞ´Ğ°Ğ»ĞµĞ½Ğ¸Ñ Ğ¸Ğ· Ñ‡Ñ‘Ñ€Ğ½Ğ¾Ğ³Ğ¾ ÑĞ¿Ğ¸ÑĞºĞ°.",
            )
            return

        try:
            user_id = int(message.text.strip())
        except ValueError:
            await self._send_safe_message(
                chat_id=message.chat.id,
                text="ĞĞµĞºĞ¾Ñ€Ñ€ĞµĞºÑ‚Ğ½Ñ‹Ğ¹ Ñ„Ğ¾Ñ€Ğ¼Ğ°Ñ‚. Ğ£ĞºĞ°Ğ¶Ğ¸Ñ‚Ğµ Ñ‡Ğ¸ÑĞ»Ğ¾Ğ²Ğ¾Ğ¹ ID Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ.",
            )
            return

        if not await self.is_user_blacklisted(user_id):
            await self._send_safe_message(
                chat_id=message.chat.id,
                text=f"ĞŸĞ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»ÑŒ <code>{user_id}</code> Ğ½Ğµ Ğ½Ğ°Ğ¹Ğ´ĞµĞ½ Ğ² Ñ‡Ñ‘Ñ€Ğ½Ğ¾Ğ¼ ÑĞ¿Ğ¸ÑĞºĞµ.",
            )
            return

        await self.remove_from_blacklist(user_id)
        await state.set_state(AdminStates.wait_blacklist_menu)
        await self._send_safe_message(
            chat_id=message.chat.id,
            text=f"âœ… ĞŸĞ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»ÑŒ <code>{user_id}</code> ÑƒĞ´Ğ°Ğ»Ñ‘Ğ½ Ğ¸Ğ· Ñ‡Ñ‘Ñ€Ğ½Ğ¾Ğ³Ğ¾ ÑĞ¿Ğ¸ÑĞºĞ°.",
            reply_markup=self.get_blacklist_menu(),
        )

    # ====================== ĞĞ‘Ğ ĞĞ‘ĞĞ¢ĞšĞ ĞŸĞ Ğ˜Ğ’ĞĞ¢ĞĞ«Ğ¥ Ğ¡ĞĞĞ‘Ğ©Ğ•ĞĞ˜Ğ™ ======================

    @staticmethod
    def _has_bot_command(message: Message) -> bool:
        """
        Ğ”ĞµÑ‚ĞµĞºÑ‚Ğ¸Ğ¼ Ğ½Ğ°Ğ»Ğ¸Ñ‡Ğ¸Ğµ Ğ»ÑĞ±Ğ¾Ğ¹ bot_command Ğ² entities,
        Ñ‡Ñ‚Ğ¾Ğ±Ñ‹ Ğ½Ğµ ÑÑ…Ğ¾Ğ¸Ñ‚ÑŒ /start, /admin Ğ¸ Ğ¿Ñ€Ğ¾Ñ‡Ğ¸Ğµ ĞºĞ¾Ğ¼Ğ°Ğ½Ğ´Ñ‹.
        """
        if not message.entities:
            return False
        for ent in message.entities:
            if isinstance(ent, MessageEntity) and ent.type == "bot_command":
                return True
            if not isinstance(ent, MessageEntity) and getattr(ent, "type", None) == "bot_command":
                return True
        return False

    async def handle_private_message(self, message: Message, state: FSMContext) -> None:
        """
        ĞĞ±Ñ‰Ğ¸Ğ¹ Ğ¾Ğ±Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‡Ğ¸Ğº Ğ¿Ñ€Ğ¸Ğ²Ğ°Ñ‚Ğ½Ñ‹Ñ… ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğ¹ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ.
        ĞšĞ¾Ğ¼Ğ°Ğ½Ğ´Ñ‹ Ğ¸Ğ³Ğ½Ğ¾Ñ€Ğ¸Ñ€ÑƒĞµÑ‚ (Ğ¸Ñ… Ğ»Ğ¾Ğ²ÑÑ‚ cmd_start/cmd_admin).
        """
        # ĞĞµ Ñ‚Ñ€Ğ¾Ğ³Ğ°ĞµĞ¼ ĞºĞ¾Ğ¼Ğ°Ğ½Ğ´Ñ‹, Ñ‡Ñ‚Ğ¾Ğ±Ñ‹ Ğ½Ğµ ÑÑ…Ğ¾Ğ¸Ñ‚ÑŒ /admin Ğ¸ Ñ‚.Ğ¿.
        if self._has_bot_command(message):
            return

        user_id = message.from_user.id

        # Ğ§Ñ‘Ñ€Ğ½Ñ‹Ğ¹ ÑĞ¿Ğ¸ÑĞ¾Ğº
        if await self.is_user_blacklisted(user_id) and not await self.is_admin(user_id):
            await self._send_safe_message(
                chat_id=message.chat.id,
                text=self.texts.you_are_blocked,
            )
            return

        # Rate-limit Ğ½Ğ° Ğ¾Ñ‚Ğ¿Ñ€Ğ°Ğ²ĞºÑƒ Ğ¾Ñ‚Ğ²ĞµÑ‚Ğ¾Ğ²
        if not await self.ratelimiter.can_send(chat_id=message.chat.id):
            wait_for = await self.ratelimiter.wait_for_send()
            await asyncio.sleep(wait_for)

        # Ğ•ÑĞ»Ğ¸ ÑÑ‚Ğ¾ Ğ°Ğ´Ğ¼Ğ¸Ğ½
        if await self.is_admin(user_id):
            # ĞŸĞ¾ĞºĞ°Ğ·Ñ‹Ğ²Ğ°ĞµĞ¼ Ğ°Ğ´Ğ¼Ğ¸Ğ½-Ğ¿Ğ°Ğ½ĞµĞ»ÑŒ
            await self._send_safe_message(
                chat_id=message.chat.id,
                text=self.texts.admin_panel_title,
                reply_markup= await self.get_admin_menu(),
            )
            return

        oc = await self.get_openchat_settings()

        # Ğ•ÑĞ»Ğ¸ Ğ²ĞºĞ»ÑÑ‡Ñ‘Ğ½ OpenChat Ğ¸ ĞµÑÑ‚ÑŒ Ğ¿Ñ€Ğ¸Ğ²ÑĞ·Ğ°Ğ½Ğ½Ñ‹Ğ¹ Ñ‡Ğ°Ñ‚ â€” ÑˆĞ»Ñ‘Ğ¼ Ğ² Ñ‚Ğ¾Ğ¿Ğ¸ĞºĞ¸
        if oc["enabled"] and oc["chat_id"]:
            try:
                await self.forward_to_openchat(message)
            except Exception as e:
                logger.error(f"Failed to forward to OpenChat: {e}")
            # ĞœĞ¾Ğ¶Ğ½Ğ¾ Ğ´Ğ¾Ğ¿Ğ¾Ğ»Ğ½Ğ¸Ñ‚ĞµĞ»ÑŒĞ½Ğ¾ Ğ¿Ñ€Ğ¸ÑĞ»Ğ°Ñ‚ÑŒ Ğ°Ğ²Ñ‚Ğ¾Ğ¾Ñ‚Ğ²ĞµÑ‚ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ
            if await self.get_setting("autoreply_enabled") == "True":
                text = await self.get_setting("autoreply_text") or ""
                if text:
                    await self._send_safe_message(
                        chat_id=message.chat.id,
                        text=text,
                    )
            else:
                await self._send_safe_message(
                    chat_id=message.chat.id,
                    text=self.texts.message_forwarded_to_support,
                )
            return

        # Ğ•ÑĞ»Ğ¸ OpenChat Ğ½Ğµ Ğ½Ğ°ÑÑ‚Ñ€Ğ¾ĞµĞ½ â€” Ğ¸Ğ½Ñ„Ğ¾Ñ€Ğ¼Ğ¸Ñ€ÑƒĞµĞ¼ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ
        await self._send_safe_message(
            chat_id=message.chat.id,
            text=self.texts.support_not_configured,
        )

    # ====================== OPENCHAT: Ğ¡ĞĞ‘Ğ©Ğ•ĞĞ˜Ğ¯ Ğ˜ Ğ Ğ•ĞŸĞ›ĞĞ˜ ======================

    async def handle_openchat_message(self, message: Message) -> None:
        """
        ĞĞ±Ñ€Ğ°Ğ±Ğ¾Ñ‚ĞºĞ° ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğ¹ Ğ² Ñ‡Ğ°Ñ‚Ğµ OpenChat (ÑÑƒĞ¿ĞµÑ€Ğ³Ñ€ÑƒĞ¿Ğ¿Ğ° Ñ Ñ‚ĞµĞ¼Ğ°Ğ¼Ğ¸).
        Ğ˜Ğ½Ñ‚ĞµÑ€ĞµÑÑƒÑÑ‚ Ñ‚Ğ¾Ğ»ÑŒĞºĞ¾ Ñ€ĞµĞ¿Ğ»Ğ°Ğ¸ Ğ²Ğ½ÑƒÑ‚Ñ€Ğ¸ Ğ¿Ñ€Ğ¸Ğ²ÑĞ·Ğ°Ğ½Ğ½Ğ¾Ğ³Ğ¾ Ñ‡Ğ°Ñ‚Ğ°.
        """
        oc = await self.get_openchat_settings()
        if not (oc["enabled"] and oc["chat_id"] and message.chat.id == oc["chat_id"]):
            return

        # Ğ‘ĞµÑ€Ñ‘Ğ¼ Ñ‚Ğ¾Ğ»ÑŒĞºĞ¾ Ğ¾Ñ‚Ğ²ĞµÑ‚Ñ‹ Ğ½Ğ° ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ñ (reply) â€” ÑÑ‚Ğ¾ ÑĞ¸Ğ³Ğ½Ğ°Ğ» Ğ¾Ñ‚Ğ²ĞµÑ‚Ğ° ĞºĞ»Ğ¸ĞµĞ½Ñ‚Ñƒ
        if not message.reply_to_message:
            return

        # ĞĞµ Ğ¾Ğ±Ñ€Ğ°Ğ±Ğ°Ñ‚Ñ‹Ğ²Ğ°ĞµĞ¼ ÑĞ¾Ğ±ÑÑ‚Ğ²ĞµĞ½Ğ½Ñ‹Ğµ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ñ Ğ±Ğ¾Ñ‚Ğ°, Ñ‡Ñ‚Ğ¾Ğ±Ñ‹ Ğ½Ğµ Ğ·Ğ°Ñ†Ğ¸ĞºĞ»Ğ¸Ñ‚ÑŒÑÑ
        if message.from_user and message.from_user.is_bot:
            return

        await self.handle_openchat_reply(message, message.reply_to_message, oc)

    async def handle_openchat_reply(
        self, message: Message, reply_msg: Message, oc: Dict[str, Any]
    ) -> None:
        """
        Ğ ĞµĞ¿Ğ»Ğ°Ğ¹ Ğ°Ğ´Ğ¼Ğ¸Ğ½Ğ° Ğ² Ñ‚ĞµĞ¼Ğµ OpenChat â†’ Ğ¾Ñ‚Ğ²ĞµÑ‚ ĞºĞ»Ğ¸ĞµĞ½Ñ‚Ñƒ Ğ² Ğ»Ğ¸Ñ‡ĞºÑƒ.
        """
        if not self.db:
            return

        # ĞĞ°Ñ…Ğ¾Ğ´Ğ¸Ğ¼, ĞºĞ¾Ğ¼Ñƒ Ğ¾Ñ‚Ğ²ĞµÑ‡Ğ°ĞµĞ¼, Ğ¿Ğ¾ ÑĞ¾Ñ…Ñ€Ğ°Ğ½Ñ‘Ğ½Ğ½Ğ¾Ğ¼Ñƒ Ğ¼Ğ°Ğ¿Ğ¿Ğ¸Ğ½Ğ³Ñƒ
        target_user_id = await self.get_target_user_by_admin_message(
            reply_msg.chat.id, reply_msg.message_id
        )
        if not target_user_id:
            # ĞĞµÑ‚ Ğ¼Ğ°Ğ¿Ğ¿Ğ¸Ğ½Ğ³Ğ° â€” Ğ½Ğµ Ğ·Ğ½Ğ°ĞµĞ¼, ĞºĞ¾Ğ¼Ñƒ Ğ¾Ñ‚Ğ¿Ñ€Ğ°Ğ²Ğ»ÑÑ‚ÑŒ
            return

        # Ğ•ÑĞ»Ğ¸ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»ÑŒ Ğ² Ñ‡Ñ‘Ñ€Ğ½Ğ¾Ğ¼ ÑĞ¿Ğ¸ÑĞºĞµ â€” ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ñ Ğº Ğ½ĞµĞ¼Ñƒ Ğ½Ğµ Ğ¾Ñ‚Ğ¿Ñ€Ğ°Ğ²Ğ»ÑĞµĞ¼
        if await self.is_user_blacklisted(target_user_id):
            return

        # Ğ£Ğ²Ğ°Ğ¶Ğ¸Ğ¼ rate limit Ğ¿ĞµÑ€ĞµĞ´ Ğ¸ÑÑ…Ğ¾Ğ´ÑÑ‰Ğ¸Ğ¼
        if not await self.ratelimiter.can_send(chat_id=target_user_id):
            wait_for = await self.ratelimiter.wait_for_send()
            await asyncio.sleep(wait_for)

        # ĞŸĞµÑ€ĞµÑÑ‹Ğ»Ğ°ĞµĞ¼ Ğ¿Ğ¾ Ñ‚Ğ¸Ğ¿Ñƒ ĞºĞ¾Ğ½Ñ‚ĞµĞ½Ñ‚Ğ° Ñ ÑƒÑ‡Ñ‘Ñ‚Ğ¾Ğ¼ Privacy Mode
        try:
            if message.text:
                await self._send_safe_message(
                    chat_id=target_user_id,
                    text=message.text,
                )
            elif message.photo:
                await self._send_safe_photo(
                    chat_id=target_user_id,
                    file_id=message.photo[-1].file_id,
                    caption=message.caption,
                )
            elif message.document:
                await self._send_safe_document(
                    chat_id=target_user_id,
                    file_id=message.document.file_id,
                    caption=message.caption,
                )
            elif message.video:
                await self._send_safe_video(
                    chat_id=target_user_id,
                    file_id=message.video.file_id,
                    caption=message.caption,
                )
            elif message.audio:
                await self._send_safe_audio(
                    chat_id=target_user_id,
                    file_id=message.audio.file_id,
                    caption=message.caption,
                )
            elif message.voice:
                await self._send_safe_voice(
                    chat_id=target_user_id,
                    file_id=message.voice.file_id,
                    caption=message.caption,
                )
            elif message.sticker:
                await self._send_safe_sticker(
                    chat_id=target_user_id,
                    file_id=message.sticker.file_id,
                )
            else:
                await self._send_safe_message(
                    chat_id=target_user_id,
                    text=f"[{message.content_type}]",
                )
        except Exception as e:
            logger.error(
                f"Failed to send OpenChat reply to user {target_user_id}: {e}"
            )
            return

        # ĞĞ±Ğ½Ğ¾Ğ²Ğ»ÑĞµĞ¼ Ñ‚Ğ°Ğ¹Ğ¼Ğ¸Ğ½Ğ³Ğ¸/ÑÑ‚Ğ°Ñ‚ÑƒÑ Ñ‚Ğ¸ĞºĞµÑ‚Ğ°
        try:
            now = datetime.now(timezone.utc)

            ticket = await self.fetch_ticket_by_chat(
                oc["chat_id"], "", target_user_id
            )
            if not ticket:
                ticket = await self.ensure_ticket_for_user(
                    oc["chat_id"], target_user_id, ""
                )

            # ĞĞ±Ğ½Ğ¾Ğ²Ğ»ÑĞµĞ¼ Ğ²Ñ€ĞµĞ¼ĞµĞ½Ğ½Ñ‹Ğµ Ğ¼ĞµÑ‚ĞºĞ¸ Ğ¾Ñ‚Ğ²ĞµÑ‚Ğ° Ğ°Ğ´Ğ¼Ğ¸Ğ½Ğ°
            await self.db.execute(
                """
                UPDATE tickets
                   SET last_admin_reply_at = %s,
                       updated_at          = %s
                 WHERE instance_id = %s
                   AND id          = %s
                """,
                (now, now, self.instance_id, ticket["id"]),
            )

            # Ğ¤Ğ¸ĞºÑĞ¸Ñ€ÑƒĞµĞ¼ ÑÑ‚Ğ°Ñ‚ÑƒÑ "ÑĞ¾Ñ‚Ñ€ÑƒĞ´Ğ½Ğ¸Ğº Ğ¾Ñ‚Ğ²ĞµÑ‚Ğ¸Ğ»" (ğŸŸ©)
            await self.set_ticket_status(ticket["id"], "answered")
        except Exception as e:
            logger.error(f"Failed to update ticket after admin reply: {e}")


    # ====================== ĞĞ’Ğ¢Ğ-Ğ—ĞĞšĞ Ğ«Ğ¢Ğ˜Ğ• Ğ¢Ğ˜ĞšĞ•Ğ¢ĞĞ’ ======================

    async def auto_close_tickets_loop(self) -> None:
        hours = int(getattr(settings, "AUTOCLOSE_HOURS", 24))
        while not self.shutdown_event.is_set():
            try:
                cutoff = datetime.now(timezone.utc) - timedelta(hours=hours)
                rows = await self.db.fetchall(
                    """
                    SELECT id
                    FROM tickets
                    WHERE instance_id = %s
                      AND status IN ('inprogress', 'answered')
                      AND last_admin_reply_at IS NOT NULL
                      AND (
                          last_user_msg_at IS NULL
                          OR last_user_msg_at < %s
                      )
                    """,
                    (self.instance_id, cutoff),
                )
                if rows:
                    for ticket_id, in rows:
                        await self.set_ticket_status(ticket_id, "closed")
                    logger.info(f"Auto-closed {len(rows)} tickets")
            except Exception as e:
                logger.error(f"Auto-close error: {e}")
            await asyncio.sleep(3600)


    # ====================== Ğ Ğ•Ğ“Ğ˜Ğ¡Ğ¢Ğ ĞĞ¦Ğ˜Ğ¯ Ğ¥Ğ­ĞĞ”Ğ›Ğ•Ğ ĞĞ’ ======================
    def register_handlers(self) -> None:
        # Ğ¡ĞµÑ€Ğ²Ğ¸ÑĞºĞ° Ğ¿Ñ€Ğ¾ Ğ¸Ğ·Ğ¼ĞµĞ½ĞµĞ½Ğ¸Ğµ Ñ‚ĞµĞ¼Ñ‹
        self.dp.message.register(
            self.handle_forum_service_message,
            F.forum_topic_edited,
        )

        # Callback'Ğ¸ Ñ‚Ğ¸ĞºĞµÑ‚Ğ½Ğ¾Ğ¹ ĞºĞ»Ğ°Ğ²Ğ¸Ğ°Ñ‚ÑƒÑ€Ñ‹
        self.dp.callback_query.register(
            self.handle_ticket_callback,
            F.data.startswith("ticket:"),
        )

        # ĞÑ†ĞµĞ½ĞºĞ°
        self.dp.callback_query.register(
            self.handle_rating_callback,
            F.data.startswith("rating:"),
        )

        # ĞšĞ¾Ğ¼Ğ°Ğ½Ğ´Ñ‹ Ğ² Ğ¿Ñ€Ğ¸Ğ²Ğ°Ñ‚ĞºĞµ
        self.dp.message.register(
            self.cmd_start,
            CommandStart(),
            F.chat.type == ChatType.PRIVATE,
        )
        self.dp.message.register(
            self.cmd_admin,
            Command("admin"),
            F.chat.type == ChatType.PRIVATE,
        )
        self.dp.message.register(
            self.cmd_openchat_off,
            Command("openchat_off"),
            F.chat.type == ChatType.PRIVATE,
        )

        # ĞŸÑ€Ğ¸Ğ²ÑĞ·ĞºĞ° OpenChat Ğ¸Ğ· Ğ³Ñ€ÑƒĞ¿Ğ¿Ñ‹/ÑÑƒĞ¿ĞµÑ€Ğ³Ñ€ÑƒĞ¿Ğ¿Ñ‹
        self.dp.message.register(
            self.cmd_bind_openchat,
            Command("bind"),
            (F.chat.type == ChatType.SUPERGROUP) | (F.chat.type == ChatType.GROUP),
        )

        # Ğ—Ğ°Ğ´Ğ°Ñ‘Ğ¼ ÑĞ·Ñ‹Ğº (Ğ˜Ğ¡ĞŸĞĞ›Ğ¬Ğ—Ğ£Ğ•Ğœ self.dp)
        self.dp.callback_query.register(
            self.handle_language_callback,
            F.data.in_(["setup_language"]) | F.data.startswith("set_lang:"),
        )

        # OpenChat: Ğ¾Ğ±Ñ€Ğ°Ğ±Ğ¾Ñ‚ĞºĞ° ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğ¹ Ğ² ÑÑƒĞ¿ĞµÑ€Ğ³Ñ€ÑƒĞ¿Ğ¿Ğµ (Ğ´Ğ»Ñ Ñ€ĞµĞ¿Ğ»ĞµĞµĞ²)
        self.dp.message.register(
            self.handle_openchat_message,
            F.chat.type == ChatType.SUPERGROUP,
        )

        # Callback'Ğ¸ Ğ°Ğ´Ğ¼Ğ¸Ğ½-Ğ¿Ğ°Ğ½ĞµĞ»Ğ¸
        self.dp.callback_query.register(self.handle_callback)

        # Ğ¡Ğ¾ÑÑ‚Ğ¾ÑĞ½Ğ¸Ñ Ğ°Ğ´Ğ¼Ğ¸Ğ½-Ğ¿Ğ°Ğ½ĞµĞ»Ğ¸
        self.dp.message.register(
            self.handle_admin_blacklist_search,
            StateFilter(AdminStates.wait_blacklist_search),
            F.chat.type == ChatType.PRIVATE,
        )
        self.dp.message.register(
            self.handle_admin_greeting,
            StateFilter(AdminStates.wait_greeting),
            F.chat.type == ChatType.PRIVATE,
        )
        self.dp.message.register(
            self.handle_admin_autoreply,
            StateFilter(AdminStates.wait_autoreply),
            F.chat.type == ChatType.PRIVATE,
        )
        self.dp.message.register(
            self.handle_admin_blacklist_add,
            StateFilter(AdminStates.wait_blacklist_add),
            F.chat.type == ChatType.PRIVATE,
        )
        self.dp.message.register(
            self.handle_admin_blacklist_remove,
            StateFilter(AdminStates.wait_blacklist_remove),
            F.chat.type == ChatType.PRIVATE,
        )

        # ĞĞ±Ñ‰Ğ¸Ğ¹ Ğ¾Ğ±Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‡Ğ¸Ğº Ğ¿Ñ€Ğ¸Ğ²Ğ°Ñ‚Ğ½Ñ‹Ñ… ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğ¹
        self.dp.message.register(
            self.handle_private_message,
            F.chat.type == ChatType.PRIVATE,
        )

    # ====================== Ğ—ĞĞŸĞ£Ğ¡Ğš / Ğ˜ĞĞ¢Ğ•Ğ“Ğ ĞĞ¦Ğ˜Ğ¯ ======================

    async def process_update(self, update: Update) -> None:
        """
        Ğ”Ğ¾Ğ¿. Ğ¼ĞµÑ‚Ğ¾Ğ´, ĞµÑĞ»Ğ¸ Ğ²Ğ´Ñ€ÑƒĞ³ Ğ·Ğ°Ñ…Ğ¾Ñ‡ĞµÑ‚ÑÑ ĞºĞ¾Ñ€Ğ¼Ğ¸Ñ‚ÑŒ Ğ²Ğ¾Ñ€ĞºĞµÑ€ Ğ°Ğ¿Ğ´ĞµĞ¹Ñ‚Ğ°Ğ¼Ğ¸ Ğ²Ñ€ÑƒÑ‡Ğ½ÑƒÑ.
        Ğ’ polling-Ñ€ĞµĞ¶Ğ¸Ğ¼Ğµ, Ğ¿Ğ¾ ÑÑƒÑ‚Ğ¸, Ğ½Ğµ Ğ½ÑƒĞ¶ĞµĞ½, Ğ½Ğ¾ Ğ¾ÑÑ‚Ğ°Ğ²Ğ»ĞµĞ½ Ğ´Ğ»Ñ ÑĞ¾Ğ²Ğ¼ĞµÑÑ‚Ğ¸Ğ¼Ğ¾ÑÑ‚Ğ¸.
        """
        await self.dp.feed_update(self.bot, update)

    async def run(self) -> None:
        """
        Ğ¡Ñ‚Ğ°Ñ€Ñ‚ Ğ²Ğ¾Ñ€ĞºĞµÑ€Ğ°: Ğ¸Ğ½Ğ¸Ñ†Ğ¸Ğ°Ğ»Ğ¸Ğ·Ğ°Ñ†Ğ¸Ñ Ğ‘Ğ”, Ğ·Ğ°Ğ¿ÑƒÑĞº Ğ°Ğ²Ñ‚Ğ¾Ğ·Ğ°ĞºÑ€Ñ‹Ñ‚Ğ¸Ñ Ğ¸ polling.
        """
        await self.init_database()
        logger.info(f"Worker started for instance {self.instance_id}")

        # Ğ¤Ğ¾Ğ½Ğ¾Ğ²Ğ°Ñ Ğ·Ğ°Ğ´Ğ°Ñ‡Ğ° Ğ°Ğ²Ñ‚Ğ¾â€‘Ğ·Ğ°ĞºÑ€Ñ‹Ñ‚Ğ¸Ñ Ñ‚Ğ¸ĞºĞµÑ‚Ğ¾Ğ²
        asyncio.create_task(self.auto_close_tickets_loop())

        # Ğ“Ğ°Ñ€Ğ°Ğ½Ñ‚Ğ¸Ñ€Ğ¾Ğ²Ğ°Ğ½Ğ½Ğ¾ ÑƒĞ±Ğ¸Ñ€Ğ°ĞµĞ¼ webhook, Ñ‡Ñ‚Ğ¾Ğ±Ñ‹ Ğ½Ğµ ĞºĞ¾Ğ½Ñ„Ğ»Ğ¸ĞºÑ‚Ğ¾Ğ²Ğ°Ñ‚ÑŒ Ñ polling
        try:
            await self.bot.delete_webhook(drop_pending_updates=True)
        except Exception as e:
            logger.warning(f"Failed to delete webhook: {e}")

        try:
            await self.dp.start_polling(self.bot)
        finally:
            self.shutdown_event.set()
            await self.bot.session.close()
            if self.db:
                self.db.close()


async def main() -> None:
    setup_logging()

    instance_id = getattr(settings, "WORKER_INSTANCE_ID", None) or os.getenv("WORKERINSTANCEID")
    token = getattr(settings, "WORKER_TOKEN", None) or os.getenv("WORKERTOKEN")

    if not instance_id or not token:
        logger.error("WORKER_INSTANCE_ID and WORKER_TOKEN must be set")
        return

    db = MasterDatabase()
    # Ğ’ĞĞ–ĞĞ: Ğ¸Ğ½Ğ¸Ñ†Ğ¸Ğ°Ğ»Ğ¸Ğ·Ğ¸Ñ€ÑƒĞµĞ¼ ÑĞ¾ĞµĞ´Ğ¸Ğ½ĞµĞ½Ğ¸Ğµ Ğ¸ ÑÑ…ĞµĞ¼Ñ‹
    await db.init()

    worker = GraceHubWorker(instance_id=instance_id, token=token, db=db)

    try:
        await worker.run()
    except asyncio.CancelledError:
        logger.info("Worker cancelled, shutting down...")
    except Exception as e:
        logger.exception(f"Worker crashed: {e}")



if __name__ == "__main__":
    asyncio.run(main())
===== ./src/shared/settings.py =====
# src/shared/settings.py
import os
from pathlib import Path

BASE_DIR = Path(os.getenv("APP_BASE_DIR", "/app"))
DATA_DIR = Path(os.getenv("DATA_DIR", str(BASE_DIR / "data")))
LOGS_DIR = Path(os.getenv("LOGS_DIR", str(BASE_DIR / "logs")))
CONFIG_DIR = Path(os.getenv("CONFIG_DIR", str(BASE_DIR / "config")))

# === Ğ‘ĞĞ—Ğ« Ğ”ĞĞĞĞ«Ğ¥ ===
