{
  "app": {
    "loading_app": "Loading",
    "title": "GraceHub Platform",
    "masterBot": "Master Bot",
    "footerBrand": "Powered by GraceHub 1.1a",
    "loading": "Loading app...",
    "error_open_from_telegram": "Open the mini app via Telegram",
    "error_owner_only": "This panel is available only to the bot owner. Open the mini app from the administrator account.",
